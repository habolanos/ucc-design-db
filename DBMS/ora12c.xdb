<o:TextTargetItem Id="o492">
<a:ObjectID>BF4A746C-85D5-401A-912C-A6A63E963FEF</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW, OPTIONS EX}

select
   v.owner,
   v.mview_name,
   decode(o.table_type, NULL, &#39;&#39;, &#39; of &#39; || o.table_name)
   ||decode (t.iot_type, &#39;&#39;, &#39;&#39;, &#39; organization index %SqlIndexOrganizedTable.&#39;||t.owner||t.table_name||&#39;%&#39;)
   ||decode(v.build_mode, &#39;PREBUILT&#39;, &#39; on prebuilt table&#39;, 
      decode (t.cluster_name, NULL, 
         decode (t.pct_free, NULL, &#39;&#39;, 0, &#39;&#39;, &#39; pctfree &#39;  ||t.pct_free)
         ||decode (t.pct_used, NULL, &#39;&#39;, 0, &#39;&#39;,  &#39; pctused &#39;  ||t.pct_used)
         ||decode (t.ini_trans, NULL, &#39;&#39;, 0, &#39;&#39;,  &#39; initrans &#39;  ||t.ini_trans)
         ||decode(nvl(t.initial_extent,nvl(t.next_extent,nvl(t.min_extents,nvl(t.max_extents,nvl(t.pct_increase,nvl(t.freelists,nvl(t.freelist_groups,NULL))))))), NULL, &#39;&#39;, &#39; storage (&#39;
            ||decode (t.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(t.initial_extent/1024)||&#39;K&#39;)
            ||decode (t.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(t.next_extent/1024)||&#39;K&#39;)
            ||decode (t.min_extents, NULL, &#39;&#39;, &#39; minextents &#39; ||t.min_extents)
            ||decode (t.max_extents, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||t.max_extents)
            ||decode (t.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||t.pct_increase)
            ||decode (t.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||t.freelists)
            ||decode (t.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||t.freelist_groups)
            ||decode (t.buffer_pool, NULL, &#39;&#39;, &#39; buffer_pool &#39; || t.buffer_pool)
            ||&#39; )&#39;)
         ||decode (t.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39;  ||t.tablespace_name)
         ||decode (t.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
-- missing column properties here
         ||decode (t.cache, &#39;Y&#39;, &#39; cache&#39;, &#39; &#39;)
         , &#39; cluster &#39;||t.cluster_name||&#39; (%SqlClustDef.&#39;||t.owner||t.table_name||&#39;%)&#39;)
      ||case when (t.partitioned = &#39;YES&#39;)
         then &#39; %SqlPartitionDef.&#39;||t.owner||t.table_name||&#39;%&#39;
         else &#39;&#39; end
      ||decode (ltrim(t.degree), &#39;1&#39;, &#39; noparallel&#39;, &#39;DEFAULT&#39;, &#39; parallel&#39;, &#39; parallel &#39;||ltrim(t.degree))
      ||case when exists(select 1 from all_lobs l where l.owner=t.owner and l.table_name=t.table_name and rownum=1)
         then &#39; %SqlLobStorage.&#39;||t.owner||t.table_name||&#39;%&#39;
         else &#39;&#39; end
   )
   ||decode (v.use_no_index, &#39;N&#39;, &#39; using no index&#39;, &#39; %SqlUsingIndexClause.&#39;||t.owner||t.table_name||&#39;%&#39;)
   ||decode (v.refresh_mode, &#39;NEVER&#39;, &#39; never refresh&#39;,    
      &#39; refresh &#39;
      ||decode (v.refresh_method, &#39;NEVER&#39;, &#39;&#39;, lower(v.refresh_method))
      || case when (r.next_date is null) then &#39;&#39; else &#39; start with &#39;&#39;&#39; || to_char(r.next_date) || &#39;&#39;&#39;&#39; end
      || case when (r.interval is null) then &#39;&#39; else &#39; next &#39; || r.interval end
      ||&#39; on &#39;||lower(v.refresh_mode)
   )
   ||decode (v.updatable, &#39;Y&#39;, &#39; for update&#39;, &#39;&#39;)
   ||case when (v.evaluation_edition is null) then &#39;&#39; else &#39; evaluate using edition &#39; || v.evaluation_edition end
   ||decode (v.rewrite_enabled, &#39;Y&#39;, &#39; enable query rewrite&#39;, &#39;N&#39;, &#39; disable query rewrite&#39;, &#39;&#39;) 
   ||case when (v.unusable_before is null) then &#39;&#39; else &#39; unusable before edition &#39; || v.unusable_before end
   ||case when (v.unusable_beginning is null) then &#39;&#39; else &#39; unusable beginning with edition &#39; || v.unusable_beginning end
   as coln
from
   all_mviews v
      left outer join all_object_tables o on (o.table_name = v.mview_name and o.owner = v.owner)
      left outer join all_tables t on (t.table_name = v.mview_name and t.owner = v.owner and nvl(t.dropped, &#39;NO&#39;) = &#39;NO&#39;)
      left outer join all_refresh r on (r.rowner = v.owner and r.rname = v.mview_name)
where 1 = 1
[  and v.owner = %.q:OWNER% ]
[  and v.mview_name = %.q:MATERIALIZEDVIEW%]
