[%ISODBCUSER% ?
select distinct
   %.q:SCHEMA%,
   name,
   decode (type, &#39;FUNCTION&#39;, name, &#39;&#39;)
from
   sys.user_source
where
   type in (&#39;PROCEDURE&#39;, &#39;FUNCTION&#39;)
order by
   2, 3
:
select distinct
   owner,
   name,
   decode (type, &#39;FUNCTION&#39;, name, &#39;&#39;)
from
   sys.all_source
where
   type in (&#39;PROCEDURE&#39;, &#39;FUNCTION&#39;)
[  and owner = %.q:SCHEMA%]
order by
   1, 2, 3
