<o:TextTargetItem Id="o298">
<a:ObjectID>693C1131-9FEF-48BF-81A3-F3802FD78388</a:ObjectID>
<a:Name>SqlSubPartDef</a:Name>
<a:TextTargetItem.Value>{VAR, VAL EX}

select
   p.owner||p.table_name,
   &#39;subpartition by &#39; || lower(p.subpartitioning_type)
   ||&#39;(&#39;
   ||case when (p.subpartitioning_key_count = 1) 
      then (select c.column_name from all_subpart_key_columns c where c.owner = p.owner and c.name = p.table_name and c.object_type = &#39;TABLE&#39;)
      else &#39;%SqlSubPartColumnList.&#39;|| p.owner || &#39;-&#39; || p.table_name || &#39;%&#39;
     end
   ||&#39;)&#39;
   ||case when exists(select 1 from all_subpartition_templates t where t.user_name = p.owner and t.table_name = p.table_name)
      then case (p.subpartitioning_type) 
         when &#39;LIST&#39; then &#39; subpartition template (%SqlSubPartListTemplateQry.&#39; || p.owner||&#39;-&#39;||p.table_name|| &#39;%)&#39;
         when &#39;RANGE&#39; then &#39; subpartition template (%SqlSubPartRangeTemplateQry.&#39; || p.owner||&#39;-&#39;||p.table_name|| &#39;%)&#39;
         else &#39;&#39; end
      else &#39;&#39; end
   as coln
from
   all_part_tables p
   join all_mview_logs l on (l.log_owner = p.owner and l.log_table = p.table_name)
where p.subpartitioning_type != &#39;NONE&#39;
[  and l.log_table = %.q:MATERIALIZEDVIEWLOG%]
</a:TextTargetItem.Value>
