<a:TemplateTargetItem.Value>.vbscript()
   Dim obj, sResult
   sResult = &quot;&quot;
   Set obj = ActiveSelection.Item(0)
   if Not(obj is Nothing) then
      Dim PhysDttpItem, sUnavailableDttp
      Set PhysDttpItem = GetPhysDataType(obj.Model.DBMS.TargetObject)
      if Not(PhysDttpItem is Nothing) then
         Dim sDttpLine, iSeparatorPos
         For each sDttpLine in Split(PhysDttpItem.Value, vbCrLf)
            if (Left(sDttpLine, 1) &lt;&gt; &quot;*&quot;) then
               if (sResult &lt;&gt; &quot;&quot;) then sResult = sResult &amp; &quot;;&quot;
               iSeparatorPos = InStr(sDttpLine, &quot;=&quot;)
               if (iSeparatorPos &gt; 0) then
                  sDttpLine = Left(sDttpLine, iSeparatorPos - 1)
               end if
               for each sUnavailableDttp in Split(&quot;long;long raw;ref;blob;clob;bfile;nclob;&quot;, &quot;;&quot;)
                  if (lcase(sDttpLine) = sUnavailableDttp) then
                     Exit For
                  end if
               next
               if (sUnavailableDttp = &quot;&quot;) then
                  sResult = sResult &amp; sDttpLine
               end if
            end if
         Next
      end if
   end if
   ScriptResult = sResult

Function GetPhysDataType(Categ)
   if (Categ.Name = &quot;PhysDataType&quot;) then
      Set GetPhysDataType = Categ
   else
      Dim sCat, pCat
      Set pCat = Nothing
      For each sCat in Categ.Categories
         Set pCat = GetPhysDataType(sCat)
         if Not(pCat is Nothing) then Exit For
      Next
      Set GetPhysDataType = pCat
   End If

End Function

.endvbscript()
