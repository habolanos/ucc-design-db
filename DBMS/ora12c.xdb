<a:ObjectID>1053E22E-A214-4669-9AA5-2892B1F1F977</a:ObjectID>
<a:Name>Options</a:Name>
<a:TextTargetItem.Value>&lt;physical_attribute_clause&gt; : composite=yes
{
   pctfree %d
   pctused %d
   initrans %d
storage : category=storage, composite=yes, parenthesis=yes
{
initial %d : default=10K
next %d : default=10K
minextents %d : default=1
maxextents %d
maxextents unlimited
pctincrease %d : default=50
freelists %d : default=1
freelist groups %d : default=1
buffer_pool %s : list=default|keep|recycle, default=default
}
}
size %s
tablespace %s : category=tablespace
&lt;cluster_type&gt; : composite=yes
{
   index
   &lt;hashkeys_clause&gt; : composite=yes
   {
      single table
      hash is %s
      hashkeys %d
   }
}
&lt;parallel_clause&gt; : composite=yes
{
   noparallel
   parallel %d
}
&lt;rowdependency&gt; %s : list=norowdependencies|rowdependencies
&lt;cache&gt; %s : list=cache|nocache
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o461">
<a:ObjectID>082B4E6E-C56E-4F62-9A4A-D5B8B79EBD95</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, CLUSTER, PhysicalOptions EX}

select
   t.owner,
   t.cluster_name,
   decode  (t.pct_free,  NULL, &#39;&#39;, 0, &#39;&#39;, &#39; pctfree &#39;  ||t.pct_free)
   ||decode(t.pct_used,  NULL, &#39;&#39;, 0, &#39;&#39;, &#39; pctused &#39;  ||t.pct_used)
   ||decode(t.ini_trans, NULL, &#39;&#39;, 0, &#39;&#39;, &#39; initrans &#39; ||t.ini_trans)
   ||&#39; storage (&#39;
      ||decode (t.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(t.initial_extent/1024)||&#39;K&#39;)
      ||decode (t.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(t.next_extent/1024)||&#39;K&#39;)
      ||decode (t.min_extents, NULL, &#39;&#39;, &#39; minextents &#39; ||t.min_extents)
      ||decode (t.max_extents, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||t.max_extents)
      ||decode (t.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||t.pct_increase)
      ||decode (t.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||t.freelists)
      ||decode (t.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||t.freelist_groups)
      ||&#39; buffer_pool &#39; || lower(t.buffer_pool)
      ||&#39; )&#39;
   ||case when (t.key_size is null) then &#39;&#39; else &#39; size &#39; || t.key_size end
   ||&#39; tablespace &#39; || t.tablespace_name
   ||case (t.cluster_type)
      when &#39;INDEX&#39; then &#39; index&#39;
      when &#39;HASH&#39; then 
           case when (trim(t.single_table) = &#39;Y&#39;) then &#39; single table&#39; else &#39;&#39; end
         ||case when (t.hashkeys &gt; 0) then &#39; hashkeys &#39; || t.hashkeys else &#39;&#39; end
         ||case (t.function) when &#39;HASH EXPRESSION&#39; then &#39; hash is %SqlHashExpr.&#39; || t.owner || &#39;_&#39; || t.cluster_name || &#39;%&#39; end
     end
   ||decode (ltrim(t.degree), &#39;1&#39;, &#39; noparallel&#39;, &#39;DEFAULT&#39;, &#39; parallel&#39;, &#39; parallel &#39;||ltrim(t.degree))
   ||decode (t.dependencies, &#39;ENABLED&#39;, &#39; rowdependencies&#39;, &#39; norowdependencies&#39;)
   ||decode (trim(t.cache), &#39;Y&#39;, &#39; cache&#39;, &#39; nocache&#39;)
   as coln
from
   all_clusters t
where 1=1
[  and t.owner=%.q:OWNER% ]
[  and t.cluster_name=%.q:CLUSTER%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
