<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>This check ensures that the Rely attribute is authorized for the reference</a:HelpMessage>
<a:OutputMessage>The following references must not have Rely attribute set:</a:OutputMessage>
<a:CheckScript>Function %Check%(obj)
   if (obj.GetExtendedAttribute(&quot;Rely&quot;)) then
      if(obj.ParentKey is Nothing) then
         %Check% = False
      else
         %Check% = (LCase(obj.ParentKey.GetExtendedAttribute(&quot;ConstraintStateRelyClause&quot;)) = &quot;rely&quot;)
      end if
   else
      %Check% = True
   end if
End Function</a:CheckScript>
<a:AutoFixScrpt>Function %Fix%(obj, outmsg)

   obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Rely&quot;, false
   outmsg = &quot;Rely attribute for reference reset.&quot;

   %Fix% = True
End Function</a:AutoFixScrpt>
<a:EnableAutoFix>1</a:EnableAutoFix>
</o:CustomCheckTargetItem>
