<o:MethodTargetItem Id="o708">
<a:ObjectID>B0AC2725-09B0-473D-AF93-496528E81C18</a:ObjectID>
<a:Name>CreateClusterObject</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1369238710</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   output &quot;Creating cluster object for index &quot; &amp; obj
   &#39; Create a cluster based on table name
   Dim Clus : Set Clus = obj.Table.Folder.ExtendedObjects.CreateNew()
   Clus.Stereotype = &quot;Cluster&quot;
   Clus.SetNameAndCode &quot;C_&quot; &amp; obj.Table.Name, &quot;C_&quot; &amp; obj.Table.Code, True
   Dim lcol : lcol = &quot;&quot;

   &#39; ... with index columns
   if (obj.IndexColumns.Count &gt; 0) then
      Dim cols, ccol, icol
      Set cols = Clus.GetExtendedCollection(&quot;Columns&quot;)
      for each icol in obj.IndexColumns
         Set ccol = cols.CreateNew()
         ccol.SetNameAndCode icol.Name, icol.Code, True
         ccol.SetExtendedAttribute &quot;Datatype&quot;, icol.Column.Datatype
         if (lcol = &quot;&quot;) then 
            lcol = ccol.Code 
         else 
            lcol = lcol &amp; &quot;,&quot; &amp; ccol.Code
         end if
      next
   end if
   
   &#39; Create the index on cluster      
   Dim Idxs : Set Idxs = Clus.GetExtendedCollection(&quot;Indexes&quot;)
   Dim CIdx : Set CIdx = Idxs.CreateNew()
   CIdx.SetNameAndCode obj.Name, obj.Code, True
   CIdx.Comment = obj.Comment
   CIdx.SetExtendedAttributeText &quot;PhysicalOptions&quot;, obj.PhysicalOptions
   
   &#39; Set tables physical options cluster part.
   obj.Table.SetPhysicalOptionSql &quot;&lt;physical_properties&gt;/&lt;cluster&gt;/cluster&quot;, Clus.Code &amp; &quot; (&quot; &amp; lcol &amp; &quot;)&quot;

   &#39; Add dependency from table to cluster
   Dim CreateAfter
   Set CreateAfter = obj.Table.Model.ChildExtendedDependencies.CreateNew
   Set CreateAfter.InfluentObject = Clus
   Set CreateAfter.DependentObject = obj.Table
   CreateAfter.Stereotype = &quot;DBCreateAfter&quot;
   
   &#39; Finally delete table &quot;cluster&quot; index
   obj.Delete

End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
