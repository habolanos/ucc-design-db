<o:TextTargetItem Id="o136">
<a:ObjectID>821EF8A3-F588-4963-B868-DFDCF766C729</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, DTTPCODE, LENGTH, SIZE, PREC, NOTNULL EX, Invisible, DefaultOnNull, COMPUTE, DEFAULT, TimeRangeColumns EX, COMMENT, ExtNotNullConstraintName EX, ExtNotNullDeferOption EX, Encrypted, Algorithm, EncryptionWithSalt}

select
   c.owner,
   c.table_name,
   c.column_name,
   case when (c.data_type_owner is null) then c.data_type else
      &#39;%SqlColADT.&#39;||c.column_name||&#39;.&#39;||c.table_name||&#39;.&#39;||c.owner||&#39;%&#39; end
   end,
   case when (c.data_type_owner is not null) then &#39;&#39; else
      case when (c.character_set_name is null) then 
         to_char(case (c.data_type) when &#39;RAW&#39; then c.data_length when &#39;UROWID&#39; then c.data_length else c.data_precision end)
      else 
         case when (c.char_used = &#39;C&#39;) then 
            case when (c.character_set_name = &#39;CHAR_CS&#39;) then 
               to_char(c.char_length) || &#39; char&#39; 
            else 
               to_char(c.char_length) 
            end
         else 
            to_char(c.char_col_decl_length) 
         end
      end
   end,
   c.data_precision,
   decode(c.data_precision, NULL, NULL, c.data_scale),
   decode(c.nullable, &#39;N&#39;, &#39;NOT NULL&#39;, [%ISDBAUSER%?&#39;%SqlGetNotNullConstName.&#39;||c.owner||c.table_name||c.column_name||&#39;nullable%&#39;:&#39;&#39;]) as colnC,
   case when (c.column_id is null) then &#39;true&#39; else &#39;false&#39; end,
   case when (c.default_on_null = &#39;YES&#39;) then &#39;true&#39; else &#39;false&#39; end,
   case when (c.virtual_column = &#39;YES&#39;) then c.data_default end,
   case when (c.virtual_column = &#39;NO&#39;) then c.data_default end,
   case when (c.user_generated = &#39;NO&#39;) then &#39;%SqlTimeRangeCols.&#39;||c.column_name||&#39;.&#39;||c.table_name||&#39;.&#39;||c.owner||&#39;%&#39; end,
   m.comments,   
   [%ISDBAUSER%?&#39;%SqlGetNotNullConstName.&#39;||c.owner||c.table_name||c.column_name||&#39;%&#39;:&#39;&#39;],
   [%ISDBAUSER%?&#39;%SqlGetNotNullConstName.&#39;||c.owner||c.table_name||c.column_name||&#39;state%&#39;:&#39;&#39;],
   decode(k.encryption_alg, NULL, 0, 1),
   decode(k.encryption_alg, &#39;AES 128 bits key&#39;,&#39;AES128&#39;,&#39;AES 192 bits key&#39;,&#39;AES192&#39;,&#39;AES 256 bits key&#39;,&#39;AES256&#39;,&#39;3 Key Triple DES 168 bits key&#39;,&#39;3DES168&#39;,&#39;2 Key Double DES 112 bits key&#39;, &#39;3DES112&#39;,NULL) as colnD, 
   k.salt
from
    sys.all_tab_cols c
    left outer join sys.all_encrypted_columns k on (c.owner = k.owner and c.table_name = k.table_name and c.column_name = k.column_name)
    left outer join sys.all_col_comments m on (m.table_name = c.table_name and m.column_name = c.column_name and m.owner = c.owner) 
where (c.user_generated=&#39;YES&#39; or (c.user_generated = &#39;NO&#39; and c.hidden_column = &#39;YES&#39; and c.virtual_column = &#39;YES&#39; and c.column_id is null and c.data_type = &#39;NUMBER&#39;))
[  and c.table_name = %.q:TABLE%]
[  and c.owner=%.q:SCHEMA%]
order by
   c.owner, c.table_name, coalesce(c.column_id, 99999)</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400244775</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
