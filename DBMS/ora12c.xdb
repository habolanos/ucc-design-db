      Set MV = obj.Model.ExtendedObjects.CreateNew()
      MV.Stereotype = &quot;MaterializedViewLog&quot;
      MV.SetExtendedAttribute &quot;MasterTable&quot;, obj
      MV.SetNameAndCode obj.Name, obj.Code, True
   else
      MsgBox &quot;A materialized view log already exists for this table.&quot;
   end if

End Sub</a:MethodScript>
