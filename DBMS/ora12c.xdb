<o:TextTargetItem Id="o294">
<a:ObjectID>0159CBEA-0E45-400E-AE62-D1E84440DA7C</a:ObjectID>
<a:Name>SqlColumnList</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   l.log_table,
   c.column_name || &#39;, &#39;
from
   dba_mview_log_filter_cols c
   join all_mview_logs l on (l.master = c.name)
where not exists(select 1
   from all_cons_columns x join all_constraints k on (k.table_name = x.table_name and k.constraint_name = x.constraint_name)
   where x.column_name = c.column_name and x.table_name = l.master and k.constraint_type = &#39;P&#39;)
[  and l.log_table = %.q:EXTENDEDOBJECT%]</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
