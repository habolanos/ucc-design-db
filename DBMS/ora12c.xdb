<o:TextTargetItem Id="o498">
<a:ObjectID>E92AEB92-0FEC-4C53-85E9-CDDA8D52A49C</a:ObjectID>
<a:Name>SqlLobStorage</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   l.owner||l.table_name,
   decode (l.column_name, NULL, &#39;&#39;, &#39; lob (&#39;||l.column_name||&#39;)&#39;
      ||&#39; store as&#39;
      ||decode(l.securefile, &#39;NO&#39;, &#39; basicfile&#39;, &#39; securefile&#39;)
      ||decode(substr(l.segment_name, 1, 7), &#39;SYS_LOB&#39;, &#39;&#39;, &#39; &#39;||l.segment_name)
      ||&#39; (&#39;
         ||&#39; tablespace &#39; || l.tablespace_name
         ||&#39; chunk &#39;||l.chunk
         ||decode(l.pctversion, null, &#39;&#39;, &#39; pctversion &#39;||l.pctversion)
         ||decode(l.retention, null, &#39;&#39;, &#39; retention&#39;)
         ||decode (l.cache, &#39;NO&#39;, &#39; nocache &#39;, &#39;YES&#39;, &#39; cache &#39;)
         ||decode (l.logging, &#39;NO&#39;, &#39; nologging &#39;, &#39;YES&#39;, &#39; logging &#39;)
         ||decode (l.in_row, &#39;NO&#39;, &#39; disable&#39;, &#39;YES&#39;, &#39; enable&#39;)||&#39; storage in row&#39;
      ||&#39; )&#39;) as coln
from 
   all_lobs l
where l.partitioned = &#39;NO&#39;
[  and l.owner=%.q:OWNER% ]
[  and l.table_name=%.q:MATERIALIZEDVIEW%]
</a:TextTargetItem.Value>
