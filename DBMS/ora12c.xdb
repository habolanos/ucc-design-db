<o:ExtendedAttributeTargetItem Id="o627">
<a:ObjectID>7EA9B346-8940-4C70-9C96-48CD168F7BD5</a:ObjectID>
<a:Name>IdentityOptions</a:Name>
<a:CreationDate>1380275916</a:CreationDate>
<a:Creator>I063968</a:Creator>
<a:ModificationDate>1380282844</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>Computed attribute to get and set identity options text.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)

   dim stpl
   stpl = &quot;[ start with %StartWith%]&quot;
   stpl = stpl &amp; &quot;[ increment by %IncrementBy%]&quot;
   stpl = stpl &amp; &quot;[%HasMaxvalue%? maxvalue %MaxvalueMaxvalue%]&quot;
   stpl = stpl &amp; &quot;[%HasMaxvalue%? minvalue %MinvalueMinvalue%]&quot;
   stpl = stpl &amp; &quot;[%Cycle%? cycle: nocycle]&quot;
   stpl = stpl &amp; &quot;[%HasCache%? cache %CacheValue%]&quot;
   stpl = stpl &amp; &quot;[%Order%? order: noorder]&quot;
   

   %Get% = obj.EvaluateTextFor(stpl)
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)

   Dim rxp, itm, mch, arr
   Set rxp = new RegExp
   rxp.IgnoreCase = True
   rxp.Global = True
   itm = Replace(Replace(value, vbCr, &quot; &quot;), vbLf, &quot; &quot;)

   rxp.pattern = &quot;(?:start\s+with\s+(limit\s+value|\d+)|&quot;
   rxp.pattern = rxp.pattern &amp; &quot;increment\s+by\s+(\d+)|&quot;
   rxp.pattern = rxp.pattern &amp; &quot;maxvalue\s+(\d+)|nomaxvalue|&quot;
   rxp.pattern = rxp.pattern &amp; &quot;minvalue\s+(\d+)|nominvalue|&quot;
   rxp.pattern = rxp.pattern &amp; &quot;(cycle|nocycle)|&quot;
   rxp.pattern = rxp.pattern &amp; &quot;cache\s+(\d+)|nocache|&quot;
   rxp.pattern = rxp.pattern &amp; &quot;(order|noorder))*&quot;
   for each mch in rxp.Execute(itm)
      arr = Split(rxp.Replace(mch, &quot;$1 ; $2 ; $3 ; $4 ; $5 ; $6 ; $7&quot;), &quot; ; &quot;)
      if(arr(0) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttributeText &quot;StartWith&quot;, arr(0)
      elseif(arr(1) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttributeText &quot;IncrementBy&quot;, arr(1)
      elseif(arr(2) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;HasMaxvalue&quot;, true
         obj.SetExtendedAttributeText &quot;MaxvalueMaxvalue&quot;, arr(2)
      elseif(arr(3) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;HasMinvalue&quot;, true
         obj.SetExtendedAttributeText &quot;MinvalueMinvalue&quot;, arr(3)
      elseif(arr(4) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Cycle&quot;, (LCase(arr(4)) = &quot;cycle&quot;)
      elseif(arr(5) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Cache&quot;, true
         obj.SetExtendedAttributeText &quot;CacheValue&quot;, arr(5)
      elseif(arr(6) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Order&quot;, (LCase(arr(6)) = &quot;order&quot;)
      end if
   next

End Sub</a:SetMethodScript>
<a:Label>Options</a:Label>
</o:ExtendedAttributeTargetItem>
</c:Categories>
