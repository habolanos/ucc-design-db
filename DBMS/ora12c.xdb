<a:CreationDate>1383906763</a:CreationDate>
<a:Creator>i063968</a:Creator>
<a:ModificationDate>1383922519</a:ModificationDate>
<a:Modifier>i063968</a:Modifier>
<a:Comment>Computed attribute specifying the column validity time range.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)
   dim c1, c2, sget
   sget = &quot;&quot;
   if(obj.GetExtendedAttribute(&quot;PeriodColumn&quot;)) then
      set c1 = obj.GetExtendedAttribute(&quot;StartTimeColumn&quot;)
      if not (c1 is Nothing) then
         set c2 = obj.GetExtendedAttribute(&quot;EndTimeColumn&quot;)
         if not (c2 is Nothing) then
            sget = c1.Code &amp; &quot;, &quot; &amp; c2.Code
         end if   
      end if
   end if
   %Get% = sget
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   obj.SetExtendedAttribute &quot;PeriodColumn&quot;, True &#39; Not yet done by Reverse !
   dim arr, c1, c2
   arr = Split(value, &quot;,&quot;)
   if(UBound(arr) &lt;&gt; 1) then 
      &#39; coming from ODBC reverse 
      &#39; syntax is &quot;([start_time_column] &lt; [end_time_column]) and ([valid_time_column] &gt; 0)&quot;
      dim rxp
      Set rxp = new regexp
      rxp.Pattern = &quot;\((.*?) &lt; (.*?)\) and \((?:.*?) &gt; 0\)&quot;
      if(rxp.Test(value)) then
         arr = Split(rxp.Replace(rxp.Execute(value)(0), &quot;$1;$2&quot;), &quot;;&quot;)
      end if
      if(UBound(arr) &lt;&gt; 1) then Exit Sub &#39; Failed to match !
   end if
   set c1 = obj.Table.FindChildByCode(Trim(arr(0)), PDPDM.cls_Column, &quot;&quot;, Nothing, False)
   set c2 = obj.Table.FindChildByCode(Trim(arr(1)), PDPDM.cls_Column, &quot;&quot;, Nothing, False)

   obj.SetExtendedAttribute &quot;%CurrentTargetCode%.StartTimeColumn&quot;, c1
   obj.SetExtendedAttribute &quot;%CurrentTargetCode%.EndTimeColumn&quot;, c2


   &#39; Oracle Database creates an INVISIBLE virtual column with this name of data type NUMBER in table.
   obj.DataType = &quot;NUMBER&quot;
   obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Invisible&quot;, True


End Sub</a:SetMethodScript>
<a:Label>Hidden</a:Label>
</o:ExtendedAttributeTargetItem>
