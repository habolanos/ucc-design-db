<a:Name>SqlXMLColsTableOptions</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select 
   c.owner || &#39;.&#39; || c.table_name,
   &#39;xmltype &#39; || c.column_name
   || &#39; store as &#39; || m.DATA_TYPE
   || &#39; (&#39;
      || &#39; tablespace &#39; || l.tablespace_name
      || decode (l.cache, &#39;YES&#39;, &#39; cache&#39;, &#39; nocache&#39;)
      || decode (l.logging, &#39;YES&#39;, &#39; logging&#39;, &#39; nologging&#39;)
      || decode (l.pctversion, NULL, &#39;&#39;, &#39; pctversion &#39;||l.pctversion)
      || decode (l.in_row, &#39;YES&#39;, &#39; enable storage in row&#39;, &#39; disable storage in row&#39;)
      || &#39; chunk &#39; || l.chunk
   || &#39; ), &#39;
from all_tab_cols c 
   join all_tab_cols m on (m.owner = c.owner and m.table_name = c.table_name and m.column_id = c.column_id and m.user_generated = &#39;NO&#39;)
   join all_lobs l on (l.owner = m.owner and l.table_name = m.table_name and l.column_name = m.column_name)
where 
   c.user_generated = &#39;YES&#39;
[  and  c.owner = %.q:OWNER%]
[  and c.table_name = %.q:TABLE%]
order by 1, m.column_id
</a:TextTargetItem.Value>
