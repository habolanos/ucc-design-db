<a:ObjectID>6DEFADA1-625C-4D8C-9762-37237813F43B</a:ObjectID>
<a:Name>AfterDatabaseReverseEngineer</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1281606245</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %AfterDatabaseReverseEngineer%(model)
   Dim O
   For each O in model.Tables
      O.ExecuteCustomMethod(&quot;%CurrentTargetCode%.UpdateXMLVirtualColumnCollection&quot;)
   next
   For each O in model.GetCollectionByStereotype(&quot;MaterializedView&quot;)
      O.Type = &quot;MATERIALIZED&quot;
   Next
   %AfterDatabaseReverseEngineer% = True
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
<o:EventHandlerTargetItem Id="o1044">
<a:ObjectID>D540FF9D-FA9D-4A9C-887F-771F1029E9FA</a:ObjectID>
<a:Name>OnModelOpen</a:Name>
<a:CreationDate>1261149361</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286808829</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %OnModelOpen%(model)

   if Not(model.GetExtendedAttribute(&quot;AutoFixMaterializedViewDone&quot;)) then
      model.ExecuteCustomMethod(&quot;%CurrentTargetCode%.AutoFixMaterializedViews&quot;)
      model.SetExtendedAttributeText &quot;AutoFixMaterializedViewDone&quot;, &quot;True&quot;
   end if

   if Not(model.GetExtendedAttribute(&quot;AutoFixTempAttributeDone&quot;)) then
      model.ExecuteCustomMethod(&quot;%CurrentTargetCode%.AutoFixTempAttribute&quot;)
      model.SetExtendedAttributeText &quot;AutoFixTempAttributeDone&quot;, &quot;True&quot;
   end if
   
   %OnModelOpen% = True
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
<o:EventHandlerTargetItem Id="o1045">
