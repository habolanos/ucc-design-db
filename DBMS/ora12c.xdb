<o:TextTargetItem Id="o81">
<a:ObjectID>057B9620-2A67-4D00-B053-5171C63F3B22</a:ObjectID>
<a:Name>SqlIndexOrganizedTable</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select
   i.table_owner||i.table_name,
   decode (i.pct_free, NULL, &#39;&#39;, 0, &#39;&#39;, &#39; pctfree &#39; || i.pct_free)
   ||decode (i.ini_trans, NULL, &#39;&#39;, 0, &#39;&#39;, &#39; initrans &#39; || i.ini_trans)
   ||decode(coalesce(i.initial_extent, i.next_extent, i.min_extents, i.max_extents, i.pct_increase, i.freelists, i.freelist_groups), NULL, &#39;&#39;, &#39; storage (&#39;
      ||decode (i.initial_extent, NULL, &#39;&#39;, &#39; initial &#39;|| (i.initial_extent/1024)||&#39;K&#39;)
      ||decode (i.next_extent, NULL, &#39;&#39;, &#39; next &#39;|| (i.next_extent/1024)||&#39;K&#39;)
      ||decode (i.min_extents, NULL, &#39;&#39;, &#39; minextents &#39;|| i.min_extents)
      ||decode (i.max_extents, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39;|| i.max_extents)
      ||decode (i.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39;|| i.pct_increase)
      ||decode (i.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||i.freelists)
      ||decode (i.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||i.freelist_groups)
      ||decode (i.buffer_pool, NULL, &#39;&#39;, &#39; buffer_pool &#39; || i.buffer_pool)
      ||&#39; )&#39;)
   ||decode (i.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39; || i.tablespace_name)
   ||decode (i.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
   ||&#39; pctthreshold &#39; || i.pct_threshold
   ||decode(i.compression, &#39;DISABLED&#39;, &#39; nocompress&#39;, &#39; compress &#39; || i.prefix_length)
   ||decode(i.include_column, &#39;0&#39;, &#39;&#39;, &#39; including &#39;|| (select c.column_name from all_tab_columns c where c.table_name = i.table_name and c.owner = i.table_owner and c.column_id = i.include_column))
   ||decode(o.table_name, NULL, &#39;&#39;, &#39; overflow &#39;
      ||&#39; pctfree &#39; || nvl(o.pct_free, p.def_pct_free)
      ||&#39; initrans &#39; || nvl(o.ini_trans, p.def_ini_trans)
      ||&#39; storage (&#39;
         ||decode (nvl(to_char(o.initial_extent), p.def_initial_extent), &#39;DEFAULT&#39;, &#39;&#39;, &#39; initial &#39;|| nvl(to_char(o.initial_extent), p.def_initial_extent))
         ||decode (nvl(to_char(o.next_extent), p.def_next_extent), &#39;DEFAULT&#39;, &#39;&#39;, &#39; next &#39;|| nvl(to_char(o.next_extent), p.def_next_extent))
         ||decode (nvl(to_char(o.min_extents), p.def_min_extents), &#39;DEFAULT&#39;, &#39;&#39;, &#39; minextents &#39;|| nvl(to_char(o.min_extents), p.def_min_extents))
         ||decode (nvl(to_char(o.max_extents), p.def_max_extents), &#39;DEFAULT&#39;, &#39;&#39;, &#39; maxextents &#39;|| nvl(to_char(o.max_extents), p.def_max_extents))
         ||decode (nvl(to_char(o.pct_increase), p.def_pct_increase), &#39;DEFAULT&#39;, &#39;&#39;, &#39; pctincrease &#39;|| nvl(to_char(o.pct_increase), p.def_pct_increase))
         ||decode (nvl(o.freelists, p.def_freelists), NULL, &#39;&#39;, &#39; freelists &#39; || nvl(o.freelists, p.def_freelists))
         ||decode (nvl(o.freelist_groups, p.def_freelist_groups), NULL, &#39;&#39;, &#39; freelist groups &#39; || nvl(o.freelist_groups, p.def_freelist_groups))
         ||&#39; buffer_pool &#39; || nvl(o.buffer_pool, p.def_buffer_pool)
      ||&#39; )&#39;
      ||&#39; tablespace &#39; || nvl(o.tablespace_name, p.def_tablespace_name)
      ||decode (nvl(o.logging, p.def_logging), &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
   )
   as coln
from 
   all_indexes i
   left outer join all_tables o on (o.iot_type = &#39;IOT_OVERFLOW&#39; and o.iot_name = i.table_name and o.owner = i.table_owner)
   left outer join all_part_tables p on (p.table_name = o.table_name and p.owner = o.owner)
where i.index_type like &#39;IOT%&#39;
   and nvl(i.dropped, &#39;NO&#39;) = &#39;NO&#39;
[  and i.table_owner=%.q:OWNER% ]
[  and i.table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1337844753</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o82">
<a:ObjectID>FE6323AD-6C39-4DE2-BC34-C237E5F18DDB</a:ObjectID>
<a:Name>SqlHVQuery</a:Name>
<a:TextTargetItem.Value>{VAR1NAME, VAR1VALUE}

select
   &#39;HighVal&#39;||tp.partition_position,
   tp.high_value
from
   all_tab_partitions tp
where 1=1
[  and tp.table_owner=%.q:OWNER% ]
[  and tp.table_name=%.q:TABLE%]</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse extra physical option data</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o83">
<a:ObjectID>0BC1CB26-C61E-4267-BA07-423636104A13</a:ObjectID>
<a:Name>SqlPartitionDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   pt.owner||pt.table_name,
   case(pt.PARTITIONING_TYPE)
      when &#39;RANGE&#39; then 
         &#39;partition by range (%SqlPartColumnList.&#39;||pt.owner||pt.table_name||&#39;%) &#39;
         ||case when (pt.interval is not null) then &#39; interval (&#39; || pt.interval || &#39;) store in (&#39; || pt.def_tablespace_name || &#39;)&#39; else &#39;&#39; end
         ||case when (pt.def_subpartition_count &gt; 0) then &#39; %SqlSubPartDef.&#39;||pt.owner||pt.table_name||&#39;%&#39; else &#39;&#39; end
         ||&#39; (%SqlRangePartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;LIST&#39; then 
         &#39;partition by list (%SqlPartColumnList.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
         ||case when (pt.def_subpartition_count &gt; 0) then &#39; %SqlSubPartDef.&#39;||pt.owner||pt.table_name||&#39;%&#39; else &#39;&#39; end
         ||&#39;(%SqlListPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;HASH&#39; then &#39;partition by hash (%SqlPartColumnList.&#39;||pt.owner||pt.table_name||&#39;%) (%SqlHashPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;REFERENCE&#39; then &#39;partition by reference (&#39; ||pt.ref_ptn_constraint_name ||&#39;) (%SqlRefrPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;SYSTEM&#39; then &#39;partition by system (%SqlRefrPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      else &#39;&#39;
   end as coln
from
   all_part_tables pt
where 1=1
[  and pt.owner=%.q:OWNER% ]
[  and pt.table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
