<a:ObjectID>9B9EB57E-C73B-4782-AA8A-D7757A28B58F</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, TRIGGER, TRGDESC, TRGBODY}

select 
   T.table_owner,
   T.table_name,
   T.owner,
   T.trigger_name,
   &#39;create trigger&#39;
   ||&#39; \[%QUALIFIER%\]%TRIGGER% &#39;
   ||&#39; \[%.l:TRGTIME%=compound?for %TRGEVENT%\[ of %COLUMNS%\] on \[%TABLQUALIFIER%\]%TABLE%:\[ %TRGTIME%\]&#39;
   ||&#39; \[ %TRGEVENT%\[ of %COLUMNS%\]\]&#39;
   ||&#39; on \[%TABLQUALIFIER%\]%TABLE%\]&#39;
   || decode(T.referencing_names, NULL, &#39;&#39;, &#39; &#39; || T.referencing_names)
   || decode(T.when_clause, NULL, &#39;&#39;, &#39; when (&#39; || T.when_clause || &#39;)&#39;) 
   as trgdesc,
   T.trigger_body
from 
   sys.all_triggers T
where
   trim(both from T.base_object_type) in (&#39;TABLE&#39;,&#39;VIEW&#39;)
[  and T.table_owner=%.q:SCHEMA%]
[  and T.table_name=%.q:TABLE%]
order by 1, 2
</a:TextTargetItem.Value>
