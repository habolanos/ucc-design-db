<o:TextTargetItem Id="o501">
<a:ObjectID>E3929167-79A3-4BDC-B7A7-2275044F398A</a:ObjectID>
<a:Name>SqlPartSubPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   s.table_owner||&#39;-&#39;||s.table_name||&#39;-&#39;||s.partition_name,
   decode(s.subpartition_position, 1, &#39;&#39;, &#39;,&#39;)
   ||&#39; subpartition &#39; || s.subpartition_name
   ||case(p.subpartitioning_type)
       when &#39;LIST&#39; then &#39; values (%SqlSubpartHighBoundQuery.&#39; || s.table_owner || &#39;-&#39; || s.table_name || &#39;-&#39; || s.subpartition_name || &#39;%)&#39;
       when &#39;RANGE&#39; then &#39; values less than (%SqlSubpartHighBoundQuery.&#39; || s.table_owner || &#39;-&#39; || s.table_name || &#39;-&#39; || s.subpartition_name || &#39;%)&#39;
     else &#39;&#39; end
   ||&#39; tablespace &#39;  ||s.tablespace_name
   ||case when (s.compression = &#39;ENABLED&#39;) then &#39; compress&#39;
      ||case (s.compress_for) 
         when &#39;FOR ALL OPERATIONS&#39; then &#39; for all operations&#39;
         when &#39;DIRECT LOAD ONLY&#39; then &#39; for direct_load operations&#39;
        else &#39;&#39; end
     else &#39; nocompress&#39; end
   ||&#39; %SqlSubPartLobStorage.&#39;||s.subpartition_name||&#39;% &#39;
   as coln
from 
   all_tab_subpartitions s
   join all_part_tables p on (p.owner = s.table_owner and p.table_name = s.table_name)
where 1=1
[  and s.table_owner=%.q:OWNER% ]
[  and s.table_name=%.q:MATERIALIZEDVIEW%]
order by
   1, s.subpartition_position</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
