<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TRIGGER, TRGSCOPEOWNER, SCRIPT EX}

select 
   owner,
   trigger_name,
   decode(trim(trailing from base_object_type), &#39;DATABASE&#39;, &#39;&#39;, table_owner),
   &#39;create trigger &#39; || description
   || decode(when_clause, NULL, &#39;&#39;, &#39; when (&#39;||when_clause||&#39;)&#39;)
   || &#39; %SqlRevBody.&#39; || owner || &#39;_&#39; || trigger_name || &#39;%&#39;  as trgscript
from 
   sys.all_triggers
where
   trim(trailing from base_object_type) in (&#39;SCHEMA&#39;,&#39;DATABASE&#39;)
[  and trigger_name=%.q:TRIGGER%]
[  and owner=%.q:OWNER%]
order by 1, 2
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
