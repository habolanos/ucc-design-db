<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>Browse among existing references to select option.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   
   Dim pSelct : Set pSelct = obj.Model.CreateSelection()
   if (pSelct is Nothing) then
      output &quot;Unable to create a selection...&quot;
      exit sub
   end if

   Dim pExt : set pExt = pSelct.ShowObjectPicker (&quot;Table::Reference&quot;, &quot;&quot;, &quot;Select reference&quot;, False)
   if (pExt is Nothing) then
      output &quot;Selection cancelled.&quot;
   else
      obj.SetExtendedAttributeText &quot;%CurrentTargetCode%.ReferencePartitionConstraint&quot;, pExt.ForeignKeyConstraintName
   End If
   Set pSelct = Nothing
   set pExt = Nothing
   
End Sub</a:MethodScript>
