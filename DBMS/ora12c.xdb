<o:TransformationTargetItem Id="o1031">
<a:ObjectID>BF47FB39-9A82-4DF2-9767-51BD092A8AFD</a:ObjectID>
<a:Name>TrsfMaterializedViewStereotype</a:Name>
<a:CreationDate>1286806807</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286808829</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:TransformationScript>Sub %Transformation%(obj, trfm)

   Dim V
   for each V in obj.Views
      if Not(V.IsShortcut or V.Replica) then
         Dim S
         Set S = trfm.GetSource(V)
         if Not(S is Nothing) then
            if(S.Type = &quot;MATERIALIZED&quot;) then
               V.Stereotype = &quot;MaterializedView&quot;
            end if
         end if
      end if
   next
   
   for each V in obj.Packages
      if Not(V.IsShortcut or V.Replica) then
         %Transformation% V, trfm
      end if
   next   

End Sub</a:TransformationScript>
</o:TransformationTargetItem>
