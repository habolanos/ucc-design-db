<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %OnLanguageChanging%(model, oldLanguage)


   if Not(model.GetExtendedAttribute(&quot;AutoFixClusterIndexesDone&quot;)) then
      model.ExecuteCustomMethod(&quot;%CurrentTargetCode%.AutoFixClusterIndexes&quot;)
      model.SetExtendedAttributeText &quot;AutoFixClusterIndexesDone&quot;, &quot;True&quot;
   end if

   if Not(model.GetExtendedAttribute(&quot;AutoFixMaterializedViewDone&quot;)) then
      model.ExecuteCustomMethod(&quot;%CurrentTargetCode%.AutoFixMaterializedViews&quot;)
      model.SetExtendedAttributeText &quot;AutoFixMaterializedViewDone&quot;, &quot;True&quot;
   end if

   %OnLanguageChanging% = True

End Function</a:HandlerScript>
