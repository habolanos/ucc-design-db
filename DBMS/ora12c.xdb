<a:TextTargetItem.Value>{OWNER, TABLE, OPTIONS EX}

select
   t.owner,
   t.table_name,
   case when (0 &lt; (select count(*) from all_xml_tab_cols c where c.owner = t.owner and c.table_name = t.table_name)) 
      then &#39; %SqlXMLColsTableOptions.&#39; || t.owner || &#39;.&#39; || t.table_name || &#39;%&#39;   else &#39;&#39;   end
   ||case when(t.temporary = &#39;Y&#39;) then 
      decode(t.duration, &#39;SYS$SESSION&#39;, &#39;on commit preserve rows&#39;, &#39;SYS$TRANSACTION&#39;, &#39;on commit delete rows&#39;, &#39;&#39;)
   else &#39;&#39; end
   ||case when (t.iot_type = &#39;IOT&#39;) then 
      &#39; organization index %SqlIndexOrganizedTable.&#39;||t.owner||t.table_name||&#39;%&#39;
   when (t.partitioned = &#39;YES&#39;) then 
      decode  (x.def_pct_free,  NULL, &#39;&#39;, &#39; pctfree &#39;  ||x.def_pct_free)
      ||decode(x.def_pct_used,  NULL, &#39;&#39;, &#39; pctused &#39;  ||x.def_pct_used)
      ||decode(x.def_ini_trans, NULL, &#39;&#39;, &#39; initrans &#39; ||x.def_ini_trans)
      ||decode(x.def_tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39;||x.def_tablespace_name)
      ||decode(x.def_logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
      ||case when (x.def_compression = &#39;ENABLED&#39;) then case (x.def_compress_for) 
         when &#39;FOR ALL OPERATIONS&#39; then &#39; compress for all operations&#39;
         when &#39;DIRECT LOAD ONLY&#39; then &#39; compress for direct_load operations&#39;
         else &#39; compress&#39; end
      else &#39; nocompress&#39; end
      || &#39; indexing &#39; || lower(x.def_indexing)
      ||&#39; %SqlPartitionDef.&#39;||t.owner||t.table_name||&#39;%&#39;
   else
      decode  (t.pct_free,  NULL, &#39;&#39;, &#39; pctfree &#39;  ||t.pct_free)
      ||decode(t.pct_used,  NULL, &#39;&#39;, &#39; pctused &#39;  ||t.pct_used)
      ||decode(t.ini_trans, NULL, &#39;&#39;, &#39; initrans &#39; ||t.ini_trans)
      ||decode(t.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39;||t.tablespace_name)
      ||decode(nvl(t.initial_extent,nvl(t.next_extent,nvl(t.min_extents,nvl(t.max_extents,nvl(t.pct_increase,nvl(t.freelists,nvl(t.freelist_groups,NULL))))))), NULL, &#39;&#39;, &#39; storage (&#39;
         ||decode (t.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(t.initial_extent/1024)||&#39;K&#39;)
         ||decode (t.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(t.next_extent/1024)||&#39;K&#39;)
         ||decode (t.min_extents, NULL, &#39;&#39;, &#39; minextents &#39; ||t.min_extents)
         ||decode (t.max_extents, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||t.max_extents)
         ||decode (t.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||t.pct_increase)
         ||decode (t.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||t.freelists)
         ||decode (t.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||t.freelist_groups)
      ||&#39; )&#39;)
      ||decode(t.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
      ||case when (t.compression = &#39;ENABLED&#39;) then case (t.compress_for) 
         when &#39;FOR ALL OPERATIONS&#39; then &#39; compress for all operations&#39;
         when &#39;DIRECT LOAD ONLY&#39; then &#39; compress for direct_load operations&#39;
         else &#39; compress&#39; end
      else &#39; nocompress&#39; end
   end
   ||case when (e.table_name is null) then &#39;&#39; else
       &#39; %SqlExternalTable.&#39;||t.owner||t.table_name||&#39;%&#39;
     end
   ||case when (t.cluster_name is NULL) then &#39;&#39; else
      &#39; cluster &#39;||t.cluster_name||&#39; (%SqlClustDef.&#39;||t.owner||t.table_name||&#39;%)&#39;
     end
   ||case when exists (select 1 from all_lobs l where l.owner=t.owner and l.table_name=t.table_name and l.column_name is not null and l.partitioned = &#39;NO&#39; and rownum=1) then
       &#39; %SqlLobStorage.&#39;||t.owner||t.table_name||&#39;%&#39;
     else &#39;&#39; end
   ||case when (n.parent_table_name is null) then &#39;&#39; else
      &#39; %SqlNestedTable.&#39;||t.owner||t.table_name||&#39;%&#39;
     end
   ||decode (trim(t.cache), &#39;Y&#39;, &#39; cache&#39;, &#39; &#39;)
   ||decode (t.monitoring, &#39;YES&#39;, &#39; monitoring&#39;, &#39;&#39;)
   ||decode (ltrim(t.degree), &#39;1&#39;, &#39; noparallel&#39;, &#39;DEFAULT&#39;, &#39; parallel&#39;, &#39; parallel &#39;||ltrim(t.degree)) as coln
from
   all_tables t
   left outer join all_part_tables x on (x.owner = t.owner and x.table_name = t.table_name)
   left outer join all_nested_tables n on (n.owner=t.owner and n.parent_table_name=t.table_name)
   left outer join all_external_tables e on (e.owner=t.owner and e.table_name=t.table_name)
where nvl(t.dropped, &#39;NO&#39;) = &#39;NO&#39;
[  and t.owner=%.q:OWNER% ]
[  and t.table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400165142</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o66">
<a:ObjectID>B963A224-AE27-4FB9-B8CE-380167022584</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, CONSTNAME, CONSTRAINT}

select 
   k.owner, 
   k.table_name, 
   k.constraint_name, 
   k.search_condition
from 
   sys.all_constraints k
where 
  k.constraint_type=&#39;C&#39;
  and 1 &lt; (select count(*) from sys.all_cons_columns c 
    where c.owner=k.owner and c.table_name=k.table_name and c.constraint_name=k.constraint_name)
[ and k.owner = %.q:SCHEMA%]
[ and k.table_name = %.q:TABLE%]
order by 1,2,3
</a:TextTargetItem.Value>
