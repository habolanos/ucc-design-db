<a:CreationDate>1423045297</a:CreationDate>
<a:Creator>I063968</a:Creator>
<a:ModificationDate>1423058734</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>Options</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)

   dim tplarray, txt, tpl, idx
   tplarray = array(_
      &quot;%ExtReferenceDeferOption%&quot;,_
      &quot;[%Rely%? rely:[%R%? norely]]&quot;,_
      &quot;[%Disable%? [%Validate%?disable validate:disable[%R% novalidate]]: [%Validate%?[%R%?enable][%R%? validate]:enable novalidate]]&quot;,_
      &quot;[exceptions into %ExceptionsInto%]&quot;_
      )
   
   txt = &quot;&quot;
   for idx = 0 to UBound(tplarray)
      tpl = Trim(obj.EvaluateTextFor(tplarray(idx), &quot;%CurrentTargetCode%&quot;))
      if(tpl &lt;&gt; &quot;&quot;) then
         txt = txt &amp; &quot; &quot; &amp; tpl
      end if
   next
   
   %Get% = Mid(txt, 2)
End Function
</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)

   dim attrarray
   attrarray = array(_
      &quot;ExtReferenceDeferOption&quot;, &quot;(not\s+deferrable|initially\s+deferred\s+deferrable|initially\s+immediate\s+deferrable|deferrable\s+initially\s+deferred|initially\s+deferred|deferrable)&quot;,_
      &quot;#Rely&quot;,                    &quot;(rely|norely)&quot;,_
      &quot;#DisableValidate&quot;,        &quot;(enable|disable)\s*(validate|novalidate)&quot;,_
      &quot;ExceptionsInto&quot;,          &quot;exceptions\s+into(\w+)&quot;,_
      &quot;&quot;)
   dim rxp, mch, txt, idx, att
   set rxp = new regexp
   rxp.IgnoreCase = True
   rxp.Global = True
   txt = Replace(Replace(value, vbLf, &quot; &quot;), vbCr, &quot; &quot;)

   idx = 0
   do
      att = attrarray(idx)
      if(att = &quot;&quot;) then exit do
&#39;output &quot;test att:&quot; &amp; att
      rxp.Pattern = attrarray(idx + 1)
      if (rxp.Test(txt)) then
         dim attv
         set mch = rxp.Execute(txt)(0)
&#39;output &quot;   =&gt; match:&quot; &amp; mch
         attv = rxp.Replace(mch, &quot;$1&quot;)
         if(Left(att, 1) = &quot;#&quot;) then &#39;special case : object or standard attribute or complex expression
            Select Case(Mid(att, 2))
               Case &quot;Rely&quot; : 
                  obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Rely&quot;, (LCase(attv) = &quot;rely&quot;)
               Case &quot;DisableValidate&quot; : 
&#39;output &quot;att=&quot; &amp; att &amp; &quot;;&quot; &amp; &quot;attv=&quot; &amp; attv
                  if (LCase(attv) = &quot;disable&quot;) then
                     obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Disable&quot;, True
                     obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Validate&quot;, (LCase(rxp.Replace(mch, &quot;$2&quot;)) = &quot;validate&quot;)
                  else
                     obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Disable&quot;, False
                     obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Validate&quot;, (LCase(rxp.Replace(mch, &quot;$2&quot;)) &lt;&gt; &quot;novalidate&quot;)
                  end if
            End Select
         else
            obj.SetExtendedAttributeText &quot;%CurrentTargetCode%.&quot; &amp; att, attv
         end if
&#39;output &quot;      =&gt; set value of &quot; &amp; att &amp; &quot; to &quot; &amp; attv            
         if (mch.FirstIndex &gt; 0) then
            txt = Trim(Left(txt, mch.FirstIndex - 1)) &amp; &quot; &quot; &amp; Trim(Mid(txt, mch.FirstIndex + mch.Length + 1))
         else
            txt = Trim(Mid(txt, mch.Length + 1))
         end if
      else
         idx = idx + 2
      end if
   loop
End Sub</a:SetMethodScript>
<a:Label>Reference Options</a:Label>
</o:ExtendedAttributeTargetItem>
