<a:Name>SqlHashPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   p.table_owner||p.table_name,
   decode (p.partition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; partition &#39;  ||p.partition_name
   ||decode (p.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39;  ||p.tablespace_name)
   ||case when exists(select 1 from all_tables o where o.iot_type = &#39;IOT_OVERFLOW&#39; and o.owner = p.table_owner and o.iot_name = p.table_name) 
      then &#39; %SqlPartOverflowQry.&#39; || p.table_owner||&#39;-&#39;||p.table_name||&#39;-&#39;||p.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when (p.compression = &#39;ENABLED&#39;) then &#39; compress&#39;
      ||case (p.compress_for) 
         when &#39;FOR ALL OPERATIONS&#39; then &#39; for all operations&#39;
         when &#39;DIRECT LOAD ONLY&#39; then &#39; for direct_load operations&#39;
        else &#39;&#39; end
     else &#39; nocompress&#39; end
   ||case when exists(select 1 from all_lob_partitions l where l.table_owner = p.table_owner and l.table_name = p.table_name and l.partition_name = p.partition_name) 
      then &#39; %SqlLobPartQuery.&#39; || p.table_owner||&#39;-&#39;||p.table_name||&#39;-&#39;||p.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   as coln
from
   all_tab_partitions p
where p.high_value_length = 0
[  and p.table_owner=%.q:OWNER% ]
[  and p.table_name=%.q:MATERIALIZEDVIEW%]
order by 
   1, p.partition_position
</a:TextTargetItem.Value>
