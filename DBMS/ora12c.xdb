<a:ObjectID>99EC13E7-70F8-43CF-A7FB-95DAC7F31047</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, INDEX}

select
   i.table_owner,
   i.table_name,
   i.owner,
   i.index_name
from
   all_indexes i 
where
   not exists (select 1 from all_constraints k where k.constraint_name = i.index_name and k.owner=i.table_owner and k.table_name = i.table_name and k.constraint_type in (&#39;P&#39;, &#39;U&#39;) and rownum=1)
   and i.join_index = &#39;NO&#39;
   and i.dropped != &#39;YES&#39;
[  and i.table_owner=%.q:SCHEMA%]
[  and i.table_name=%.q:TABLE%]
order by 1,2,3,4
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
