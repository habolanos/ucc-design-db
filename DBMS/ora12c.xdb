<a:ObjectID>34D90BC7-A193-4E80-AE7A-00A5A858F7B1</a:ObjectID>
<a:Name>Definition</a:Name>
<a:CreationDate>1268839172</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1321462698</a:ModificationDate>
<a:Modifier>xwang</a:Modifier>
<a:Comment>Computed attribute used for generation and reverse.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)
   Dim pAttr, sAttr, bAttr, sttmt
   sttmt = &quot;&quot;
   
   sAttr = obj.GetExtendedAttribute(&quot;Identification&quot;)
   sttmt = sttmt &amp; vbCrLf &amp; &quot; identified &quot; &amp; sAttr
   if (sAttr = &quot;by&quot;) then
      sttmt = sttmt &amp; &quot; &quot;&quot;&quot; &amp; obj.GetAttributeText(&quot;Password&quot;) &amp; &quot;&quot;&quot;&quot;
   else
      sAttr = obj.GetExtendedAttribute(&quot;DistinguishedName&quot;)
      if (sAttr &lt;&gt; &quot;&quot;) then
         sttmt = sttmt &amp; &quot; as &#39;&quot; &amp; sAttr &amp; &quot;&#39;&quot;
      end if
   end if
   
   Set pAttr = obj.GetExtendedAttribute(&quot;DefaultTablespace&quot;)
   if Not(pAttr is Nothing) then
      sttmt = sttmt &amp; vbCrLf &amp; &quot;   default tablespace &quot; &amp; pAttr.Code
   end if
   
   Set pAttr = obj.GetExtendedAttribute(&quot;TemporaryTablespace&quot;)
   if Not(pAttr is Nothing) then
      sttmt = sttmt &amp; vbCrLf &amp; &quot;   temporary tablespace &quot; &amp; pAttr.Code
   end if

   sAttr = obj.GetExtendedAttribute(&quot;QuotaDefinition&quot;)
   if (sAttr &lt;&gt; &quot;&quot;) then
      sttmt = sttmt &amp; vbCrLf &amp; &quot;   &quot; &amp; sAttr
   end if
   
   sAttr = obj.GetExtendedAttribute(&quot;Profile&quot;)
   if (sAttr &lt;&gt; &quot;&quot;) then
      sttmt = sttmt &amp; vbCrLf &amp; &quot;   profile &quot; &amp; sAttr
   end if
   
   bAttr = obj.GetExtendedAttribute(&quot;PasswordExpire&quot;)
   if (bAttr) then
      sttmt = sttmt &amp; vbCrLf &amp; &quot;   password expire&quot;
   end if
   
   sAttr = obj.GetExtendedAttribute(&quot;AccountLock&quot;)
   if (sAttr &lt;&gt; &quot;&quot;) then
      sttmt = sttmt &amp; vbCrLf &amp; &quot;   account &quot; &amp; sAttr
   end if
   
   %Get% = sttmt
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   Dim rxp, itm, mch
   Set rxp = new RegExp
   rxp.IgnoreCase = True
   rxp.Global = True
   itm = Replace(Replace(value, vbCr, &quot; &quot;), vbLf, &quot; &quot;)

   Dim M, S

   rxp.Pattern = &quot;identified\s+(by\s+(&quot;&quot;([^&quot;&quot;]+)&quot;&quot;|[^\s]+)|(externally|globally)(\s+as\s+\&#39;([^\&#39;]+)\&#39;)?)&quot;
   For each mch in rxp.Execute(itm)
      S = rxp.Replace(mch.Value, &quot;$2&quot;)
      if (S &lt;&gt; &quot;&quot;) then
         if (Left(S, 1) = &quot;&quot;&quot;&quot;) then S = rxp.Replace(mch.Value, &quot;$3&quot;)
         obj.SetExtendedAttribute &quot;Identification&quot;, &quot;by&quot;
         obj.PassWord = S
      else
         obj.SetExtendedAttribute &quot;Identification&quot;, LCase(rxp.Replace(mch.Value, &quot;$4&quot;))
         S = rxp.Replace(mch.Value, &quot;$6&quot;)
         if (S &lt;&gt; &quot;&quot;) then
            obj.SetExtendedAttribute &quot;DistinguishedName&quot;, S
         end if
      end if
   Next


   rxp.Pattern = &quot;(default|temporary)\s+tablespace\s+(&quot;&quot;([^&quot;&quot;]+)&quot;&quot;|[^\s]+)&quot;
   For each mch in rxp.Execute(itm)
      S = rxp.Replace(mch.Value, &quot;$2&quot;)
      if (Left(S, 1) = &quot;&quot;&quot;&quot;) then
         S = rxp.Replace(mch.Value, &quot;$3&quot;)
      end if
      Set M = obj.Model.FindChildByCode(S, cls_Tablespace, &quot;&quot;, Nothing, False, True)
      if (M is Nothing) and not(ActiveModel is Nothing) then
         Set M = ActiveModel.FindChildByCode(S, cls_Tablespace, &quot;&quot;, Nothing, False, True)
      end if
      if (M is Nothing) then
         output &quot;Tablespace &quot; &amp; S &amp; &quot; not found in model&quot;
      end if
      Select Case(LCase(rxp.Replace(mch.Value, &quot;$1&quot;)))
         Case &quot;default&quot;
            obj.SetExtendedAttribute &quot;DefaultTablespace&quot;, M
         Case &quot;temporary&quot;
            obj.SetExtendedAttribute &quot;TemporaryTablespace&quot;, M
      end Select
   Next
   rxp.Pattern = &quot;profile\s+(&quot;&quot;([^&quot;&quot;]+)&quot;&quot;|[^\s]+)&quot;
   For each mch in rxp.Execute(itm)
      S = rxp.Replace(mch.Value, &quot;$1&quot;)
      if (Left(S, 1) = &quot;&quot;&quot;&quot;) then
         S = rxp.Replace(mch.Value, &quot;$2&quot;)
      end if
      obj.SetExtendedAttribute &quot;Profile&quot;, S
      exit for
   Next

   rxp.Pattern = &quot;password\s+expire&quot;
   obj.SetExtendedAttribute &quot;PasswordExpire&quot;, false
   For each mch in rxp.Execute(itm)
      obj.SetExtendedAttribute &quot;PasswordExpire&quot;, true
      Exit For
   Next

   rxp.Pattern = &quot;account\s+(lock|unlock)&quot;
   For each mch in rxp.Execute(itm)
      obj.SetExtendedAttribute &quot;AccountLock&quot;, LCase(rxp.Replace(mch.Value, &quot;$1&quot;))
      exit for
   Next
   
   Dim Q
   Q = &quot;&quot;
   rxp.Pattern = &quot;quota\s+(unlimited|\d+\s*(K|M|G|T|P|E))\s+on\s+(&quot;&quot;([^&quot;&quot;]+)&quot;&quot;|[^\s]+)&quot;
   For each mch in rxp.Execute(itm)
      if (Q &lt;&gt; &quot;&quot;) then Q = Q &amp; vbCrLf
      Q = Q &amp; mch
   Next
   obj.SetExtendedAttribute &quot;QuotaDefinition&quot;, Q

End Sub</a:SetMethodScript>
<a:ValidateDataType>0</a:ValidateDataType>
</o:ExtendedAttributeTargetItem>
