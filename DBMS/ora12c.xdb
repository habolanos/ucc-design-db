<o:TextTargetItem Id="o444">
<a:ObjectID>55EC0D92-AE4B-475B-B3CB-2BEEF7327F67</a:ObjectID>
<a:Name>SqlColumns</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, JIDXFACT ID, OWNER ID, JIDX ID, UNIQUE ID, INDEXTYPE ID, JIDXCOLN EX ...}

select
   i.table_owner,
   i.table_name,
   i.owner,
   i.index_name,
   decode(i.uniqueness, &#39;UNIQUE&#39;, &#39;unique&#39;, &#39;&#39;),
   decode(i.index_type, &#39;BITMAP&#39;, &#39;bitmap&#39;, &#39;&#39;),
   c.table_owner ||&#39;.&#39;|| c.table_name ||&#39;.&#39;|| decode(substr(c.column_name, 1, 6), &#39;SYS_NC&#39;, &#39;%SqlExpression.Xpr&#39;||i.table_name||i.index_name||c.column_position||&#39;%&#39;, c.column_name)||&#39;, &#39; as coln,
   c.column_position
from
   all_indexes i,
   all_ind_columns c
where i.join_index = &#39;YES&#39;
   and c.index_name=i.index_name
   and c.index_owner=i.owner
   and nvl(i.dropped, &#39;NO&#39;) = &#39;NO&#39;
[  and i.table_owner=%.q:OWNER%]
[  and i.table_name=%.q:JIDX%]
order by 1,2,3,4,8
</a:TextTargetItem.Value>
