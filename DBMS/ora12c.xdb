<o:MethodTargetItem Id="o1047">
<a:ObjectID>24AC2A5A-C61D-4076-974D-FCDE05DEDC25</a:ObjectID>
<a:Name>AutoFixMaterializedViews</a:Name>
<a:CreationDate>1286806807</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286808829</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   Dim V
   for each V in obj.Views
      if Not(V.IsShortcut or V.Replica) then
         if(V.Type = &quot;MATERIALIZED&quot;) then
            V.Stereotype = &quot;MaterializedView&quot;
         end if
      end if
   next
   
   for each V in obj.Packages
      if Not(V.IsShortcut or V.Replica) then
         %Method% V
      end if
   next   

End Sub


</a:MethodScript>
</o:MethodTargetItem>
<o:MethodTargetItem Id="o1048">
<a:ObjectID>1EE878B5-4D8A-43DB-95D9-7AC9198CA782</a:ObjectID>
<a:Name>AutoFixTempAttribute</a:Name>
<a:CreationDate>1286806807</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286808829</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   Dim T
   for each T in obj.Tables
      if Not(T.IsShortcut or T.Replica) then
         T.ExecuteCustomMethod(&quot;%CurrentTargetCode%.AutoFixTempAttribute&quot;)
      end if
   next
   
   for each T in obj.Packages
      if Not(T.IsShortcut or T.Replica) then
         %Method% T
      end if
   next   

End Sub</a:MethodScript>
</o:MethodTargetItem>
<o:MethodTargetItem Id="o1049">
<a:ObjectID>B44AA9F2-1F3A-4BA5-9C3F-221CF90D32D0</a:ObjectID>
<a:Name>AutoFixClusterIndexes</a:Name>
<a:CreationDate>1286808879</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1340283622</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   dim T, Idx
   for each T in obj.Tables
      if Not(T.IsShortcut or T.Replica) then
         for each Idx in T.Indexes
            If (Idx.IsKindOf(cls_Index)) then
               If (Idx.Clustered) then
                  Idx.ExecuteCustomMethod(&quot;%CurrentTargetCode%.CreateClusterObject&quot;)
               End if
            End if
         next
      end if
   next

   for each T in obj.Packages
      if Not(T.IsShortcut or T.Replica) then
         %Method% T
      end if
   next   

End Sub</a:MethodScript>
</o:MethodTargetItem>
