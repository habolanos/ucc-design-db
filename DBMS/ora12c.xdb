<a:ObjectID>9F670164-1B1E-4454-9DB4-5B909199764B</a:ObjectID>
<a:Name>SqlRevBody</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select 
   owner || &#39;_&#39; || trigger_name,
   trigger_body
from 
   sys.all_triggers
where 1=1
[  and owner=%.q:SCHEMA%]
[  and trigger_name=%.q:TRIGGER%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1248101256</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>Reverse trigger body</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
