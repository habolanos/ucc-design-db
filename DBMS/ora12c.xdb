<o:EventHandlerTargetItem Id="o1178">
<a:ObjectID>4ED3BAD6-AC91-4E53-83B3-A8E395470C4E</a:ObjectID>
<a:Name>Validate</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %Validate%(obj, ByRef message)

   &#39; Check if another mvlog is linked to the MasterTable
   Dim MT, MV
   Set MT = obj.GetExtendedAttribute(&quot;MasterTable&quot;)
   if (MT is Nothing) then
      message = &quot;Cannot create a materialized view log out of master table context.&quot;
      %Validate% = False
      Exit Function
   end if

   For each MV in obj.Model.ExtendedObjects
      if (Not MV is obj) and (MV.Stereotype = &quot;MaterializedViewLog&quot;) then
         if (MV.GetExtendedAttribute(&quot;MasterTable&quot;) is MT) then
            message = &quot;Materialized view log &quot; &amp; MV.Name &amp; &quot; (&quot; &amp; MV.Code &amp; &quot;) already has &quot; &amp; MT &amp; &quot; as master table. You cannot create a second one.&quot;
            %Validate% = False
            Exit Function
         end if
      end if
   Next
   %Validate% = True
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
