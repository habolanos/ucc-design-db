<o:MethodTargetItem Id="o936">
<a:ObjectID>97D5EB29-0C19-4EDD-A9AD-CF8814E4C16F</a:ObjectID>
<a:Name>AutoFixTempAttribute</a:Name>
<a:CreationDate>1261152177</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1269936675</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   if (obj.Type = &quot;TEMP&quot;) then Exit Sub
   if (obj.GetExtendedAttribute(&quot;ExtTableType&quot;, 2) &lt;&gt; &quot;&quot;) then
      Dim T
      
      T = obj.GetExtendedAttribute(&quot;ExtTableType&quot;, 2)
      if (LCase(T) = &quot;global temporary&quot;) then
         obj.Type = &quot;TEMP&quot;
      end if
   end if
End Sub</a:MethodScript>
</o:MethodTargetItem>
