<o:MethodTargetItem Id="o939">
<a:ObjectID>4091B17B-A711-4A50-9017-8B96723F5710</a:ObjectID>
<a:Name>UpdateXMLVirtualColumnCollection</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1381841929</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   if(obj.Type = &quot;XML&quot;) then
      Dim sitem, bSuccess
      dim pos1, pos2
      bSuccess = True
      sitem = obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.RevVirtualColumns&quot;)
      if (sitem &lt;&gt; &quot;&quot;) then
         Dim par
         Dim CurChar, CurColn
         pos1 = 1
         par = 0
         CurColn = &quot;&quot;
         do
            CurChar = Mid(sitem, pos1, 1)
            pos1 = pos1 + 1
            if (CurChar = &quot;(&quot;) then par = par + 1
            if (CurChar = &quot;)&quot;) then par = par - 1
            if (((CurChar = &quot;,&quot;) or (pos1 &gt; Len(sitem))) and (par = 0)) then 
               If (pos1 &gt; Len(sitem)) then 
                  CurColn = CurColn &amp; CurChar
               end if
               
               CurColn = Replace(CurColn, vbTab, &quot; &quot;)
               CurColn = Replace(CurColn, vbCr, &quot; &quot;)
               CurColn = Replace(CurColn, vbLf, &quot;&quot;)
               
               pos2 = InStr(LCase(CurColn), &quot; as &quot;)
               if (pos2 &gt; 0) then
                  Dim sName, sExpr
                  sName = Trim(Left(CurColn, pos2 - 1))
                  sExpr = Trim(Mid(CurColn, pos2 + 4))
                  if ((Left(sExpr, 1) = &quot;(&quot;) and (Right(sExpr, 1) = &quot;)&quot;)) then
                     sExpr = Mid(sExpr, 2, Len(sExpr) - 2)
                  end if
                  
                  Dim pColl, pXmlCol
                  Set pColl = obj.GetExtendedCollection(&quot;%CurrentTargetCode%.XMLVirtualColumns&quot;)
                  Set pXmlCol = pColl.CreateNew()
                  pXmlCol.SetNameAndCode sName, sName, True
                  pXmlCol.SetExtendedAttributeText &quot;%CurrentTargetCode%.Expression&quot;, sExpr
               Else
                  bSuccess = False
                  Exit Do
               End if
               CurColn = &quot;&quot;
            else
               CurColn = CurColn &amp; CurChar
            end if
            if (pos1 &gt; Len(sitem)) then Exit Do
         loop
         if ((bSuccess) and (CurColn = &quot;&quot;)) then
            obj.SetExtendedAttribute &quot;%CurrentTargetCode%.RevVirtualColumns&quot;, &quot;&quot;
         else
            output &quot;An error occurs while parsing XML Virtual Column for &quot; &amp; obj
         end if
      end if
      &#39; Additionally set the Schema and Element based on physical options
      sitem = obj.GetPhysicalOptionSql(&quot;&lt;xmltype_table&gt;/&lt;xmlschema_spec&gt;/xmlschema&quot;)
      if(sitem &lt;&gt; &quot;&quot;) then
         pos1 = InStr(sitem, &quot;&quot;&quot;&quot;) + 1
         pos2 = InStrRev(sitem, &quot;&quot;&quot;&quot;)
         if(pos2 &gt; pos1) then
            obj.XMLSchema = Mid(sitem, pos1, pos2 - pos1)
         end if
      end if
      sitem = obj.GetPhysicalOptionSql(&quot;&lt;xmltype_table&gt;/&lt;xmlschema_spec&gt;/element&quot;)
      if(sitem &lt;&gt; &quot;&quot;) then
         pos1 = InStr(sitem, &quot;&quot;&quot;&quot;) + 1
         pos2 = InStrRev(sitem, &quot;&quot;&quot;&quot;)
         if(pos2 &gt; pos1) then
            obj.XMLElement = Mid(sitem, pos1, pos2 - pos1)
         else
            obj.XMLElement = Trim(Replace(sitem, &quot;element&quot;, &quot;&quot;))
         end if
      end if
   end if
End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
