<a:Name>SqlClustDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

[%ISDBAUSER%?select
   k.owner||k.table_name,
   decode (c.column_id, 1, k.tab_column_name, &#39;,&#39;||k.tab_column_name),
   c.column_id
from
   dba_clu_columns k
   join all_tab_columns c on (c.owner = user and c.table_name = k.cluster_name and c.column_name = k.clu_column_name)
where 1=1
[  and k.owner = %.q:OWNER%]
[  and k.table_name = %.q:TABLE%]
order by 1, 3
:
select
   user||k.table_name,
   decode (c.column_id, 1, k.tab_column_name, &#39;,&#39;||k.tab_column_name),
   c.column_id
from
   user_clu_columns k
   join all_tab_columns c on (c.owner = user and c.table_name = k.cluster_name and c.column_name = k.clu_column_name)
where 1=1
[  and k.table_name = %.q:TABLE%]
order by 1, 3
]</a:TextTargetItem.Value>
