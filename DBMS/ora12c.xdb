<o:MethodTargetItem Id="o869">
<a:ObjectID>72CCCCB1-EB43-4FC3-B47E-B4A349489ABA</a:ObjectID>
<a:Name>PhysicalPropertiesClusterColumnEdit</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1384880708</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>Allow multiple selection of table columns to set them as cluster columns.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   Dim sClust
   sClust = obj.GetExtendedAttribute (&quot;%CurrentTargetCode%.PhysicalPropertiesCluster&quot;)
   if (sClust = &quot;&quot;) then
      MsgBox &quot;You must select a cluster before setting the column list&quot;
      Exit sub
   end if
   Dim pClust, bFound
   bFound = False
&#39; Search by code
   For each pClust in obj.Model.ExtendedObjects
      if (pClust.Stereotype = &quot;Cluster&quot;) then
         if (pClust.Code = sClust) then
            bFound = True
            Exit For
         end if
      end if
   Next
&#39; Search by code, any case
   if not(bFound) then
      For each pClust in obj.Model.ExtendedObjects
         if (pClust.Stereotype = &quot;Cluster&quot;) then
            if (lcase(pClust.Code) = lcase(sClust)) then
               bFound = True
               Exit For
            end if
         end if
      Next
   End if 
&#39; Search by name, any case
   if not(bFound) then
      For each pClust in obj.Model.ExtendedObjects
         if (pClust.Stereotype = &quot;Cluster&quot;) then
            if (lcase(pClust.Name) = lcase(sClust)) then
               bFound = True
               Exit For
            end if
         end if
      Next
   End if 
   if Not(bFound) then
      output &quot;Warning: Specified cluster cannot be found in the model&quot;
   End if 

   Dim pSelct : Set pSelct = obj.Model.CreateSelection()
   if (pSelct is Nothing) then
      output &quot;Unable to create a selection...&quot;
      exit sub
   end if
   
   Dim sSelct, pClusCol, sColn
   sSelct = obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.PhysicalPropertiesClusterColumn&quot;)
   for each sColn in Split(sSelct, &quot;,&quot;)
      set pClusCol = obj.FindChildByCode(Trim(sColn), pdpdm.cls_Column, &quot;&quot;, Nothing, False, False)
      if not(pClusCol is Nothing) then
         pSelct.Objects.Add(pClusCol)
      end if
   next   
   pSelct.ShowObjMultiSelection obj, &quot;Columns&quot;
   if (pSelct.Objects.Count &gt; 0) then
      sSelct = &quot;&quot;
      For each pClusCol in pSelct.Objects
         if (sSelct &lt;&gt; &quot;&quot;) then sSelct = sSelct &amp; &quot;, &quot;
         sSelct = sSelct &amp; pClusCol.Code &#39; Don&#39;t use generated name that is prefixed with table
      Next
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.PhysicalPropertiesClusterColumn&quot;, sSelct
   end if
End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
