<o:TextTargetItem Id="o511">
<a:ObjectID>4D5BE5DD-3130-4495-91B5-FD696FDB4A9B</a:ObjectID>
<a:Name>SqlUsingIndexClause</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select
   i.table_owner||i.table_name,
   &#39;using index &#39;
   ||decode (i.ini_trans, NULL, &#39;&#39;, 0, &#39;&#39;,  &#39; initrans &#39;  ||i.ini_trans)
   ||decode (i.initial_extent, NULL, &#39;&#39;, &#39; storage (initial &#39; ||(i.initial_extent/1024)||&#39;K&#39;
      ||decode (i.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(i.next_extent/1024)||&#39;K&#39;)
      ||decode (i.min_extents, NULL, &#39;&#39;, 1, &#39;&#39;, &#39; minextents &#39; ||i.min_extents)
      ||decode (i.max_extents, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||i.max_extents)
      ||decode (i.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||i.pct_increase)
      ||decode (i.freelists, NULL, &#39;&#39;, 1, &#39;&#39;, &#39; freelists &#39;||i.freelists)
      ||decode (i.freelist_groups, NULL, &#39;&#39;, 1, &#39;&#39;, &#39; freelist groups &#39;||i.freelist_groups)
      ||decode (i.buffer_pool, NULL, &#39;&#39;, &#39; buffer_pool &#39; || i.buffer_pool)
      ||&#39;)&#39;,
   &#39;&#39;)
   ||decode (i.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39;  ||i.tablespace_name) as coln
from 
   all_indexes i
where 1 = 1
[  and i.table_name = %.q:MATERIALIZEDVIEW%]
[  and i.table_owner = %.q:OWNER%]</a:TextTargetItem.Value>
