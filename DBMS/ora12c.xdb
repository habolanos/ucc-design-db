<o:TextTargetItem Id="o84">
<a:ObjectID>736639B3-E9C9-48F0-A07A-C124F9B90194</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE}

select
   t.owner,
   t.table_name
from
   sys.all_all_tables t
where
   not exists (select 1 from sys.all_mviews s where s.owner = t.owner and t.table_name in (s.mview_name, s.update_log))
   and t.iot_name is null
   and nvl(t.dropped, &#39;NO&#39;) = &#39;NO&#39;
   and substr(t.table_name, 1, 5) not in (&#39;MLOG$&#39;, &#39;RUPD$&#39;)
[  and t.table_name=%.q:TABLE%]
[  and t.owner=%.q:SCHEMA%]
order by
   t.owner, t.table_name
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1337844753</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o85">
<a:ObjectID>29CFDF07-9A01-4683-91CF-2A4A71E403D0</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GRANTEE ID, PERMISSION ...}

[%ISDBAUSER% ?
select s.grantee grantee,
s.privilege
||
decode (s.grantable,&#39;YES&#39;,&#39;+&#39;,&#39;NO&#39;,&#39;&#39;)
|| &#39;,&#39;
from dba_tab_privs s
where
  s.table_name = %.q:TABLE%
[and s.owner = %.q:OWNER%]
order by grantee
:
select s.grantee grantee,
s.privilege
||
decode (s.grantable,&#39;YES&#39;,&#39;+&#39;,&#39;NO&#39;,&#39;&#39;)
|| &#39;,&#39;
from user_tab_privs s
where
  s.table_name = %.q:TABLE%
[and s.owner = %.q:OWNER%]
order by grantee
]</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1337844753</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to reverse object permissions</a:Comment>
</o:TextTargetItem>
