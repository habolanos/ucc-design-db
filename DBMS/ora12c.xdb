<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW, SQL, COMMENT}

select
   v.owner,
   v.view_name,
   v.text,
   (select r.comments from sys.all_tab_comments r where r.table_name = v.view_name and r.owner = v.owner and r.table_type = &#39;VIEW&#39;)
from
   sys.all_views v
where 1=1
[  and v.owner = %.q:OWNER%]
[  and v.view_name = %.q:VIEW%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
