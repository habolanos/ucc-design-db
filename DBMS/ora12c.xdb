<o:TextTargetItem Id="o78">
<a:ObjectID>97F42B7E-34BB-4EF7-888C-40E799F3D843</a:ObjectID>
<a:Name>SqlExternalTable</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   e.owner||e.table_name,
    &#39;organization external (&#39;
    ||decode(e.type_name, &#39;ORACLE_LOADER&#39;, &#39;&#39;, &#39;type &#39;||e.type_name)
    ||&#39; default directory &#39;||default_directory_name
    || case when (length(e.access_parameters) &gt; 0) then &#39; access parameters (&#39;|| e.access_parameters ||&#39; )&#39; end
    ||&#39; location (&#39; || (select l.directory_name || &#39;:&#39;&#39;&#39; || l.location || &#39;&#39;&#39;&#39; from all_external_locations l where l.table_name = e.table_name and l.owner = e.owner) || &#39;)&#39;
    ||&#39; )&#39;
    ||&#39; reject limit &#39;||e.reject_limit as coln
from 
   all_external_tables e
where 1 = 1
[  and e.owner=%.q:OWNER% ]
[  and e.table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
