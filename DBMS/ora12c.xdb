<o:TextTargetItem Id="o503">
<a:ObjectID>16988EA6-4938-4CB4-9B98-2BE7BF44D7D6</a:ObjectID>
<a:Name>SqlRangePartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   tp.table_owner||tp.table_name,
   decode (tp.partition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; partition &#39;  ||tp.partition_name
   ||&#39; values less than (%SqlHVQuery.HighVal&#39;||tp.partition_position||&#39;%) &#39;
   ||decode (tp.pct_free, NULL, &#39;&#39;, 10, &#39;&#39;, 0, &#39;&#39;, &#39; pctfree &#39;  ||tp.pct_free)
   ||decode (tp.pct_used, NULL, &#39;&#39;, 40, &#39;&#39;, 0, &#39;&#39;,  &#39; pctused &#39;  ||tp.pct_used)
   ||decode (tp.ini_trans, NULL, &#39;&#39;, 1, &#39;&#39;, 0, &#39;&#39;,  &#39; initrans &#39;  ||tp.ini_trans)
   ||decode (tp.tablespace_name, &#39;SYSTEM&#39;, &#39;&#39;, NULL, &#39;&#39;, &#39; tablespace &#39;  ||tp.tablespace_name)
   ||decode (tp.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
   ||replace(&#39; storage (&#39;
      ||decode (tp.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(tp.initial_extent/1024)||&#39;K&#39;)
      ||decode (tp.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(tp.next_extent/1024)||&#39;K&#39;)
      ||decode (tp.min_extent, NULL, &#39;&#39;, &#39; minextents &#39; ||tp.min_extent)
      ||decode (tp.max_extent, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||tp.max_extent)
      ||decode (tp.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||tp.pct_increase)
      ||decode (tp.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||tp.freelists)
      ||decode (tp.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||tp.freelist_groups)
      ||&#39; )&#39;, &#39; storage ( )&#39;, &#39;&#39;)
   ||case (coalesce(tp.compress_for, tp.compression))
      when pt.def_compress_for then &#39;&#39;
      when pt.def_compression then &#39;&#39;
      when &#39;FOR ALL OPERATIONS&#39; then &#39; compress for all operations&#39;
      when &#39;DIRECT LOAD ONLY&#39; then &#39; compress for direct_load only&#39;
      else case when (pt.def_compression = &#39;NONE&#39;) then &#39;&#39; else &#39; nocompress&#39; end
     end
   ||case when exists(select 1 from all_tables o where o.iot_type = &#39;IOT_OVERFLOW&#39; and o.owner = tp.table_owner and o.iot_name = tp.table_name) 
      then &#39; %SqlPartOverflowQry.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when exists(select 1 from all_lob_partitions l where l.table_owner = tp.table_owner and l.table_name = tp.table_name and l.partition_name = tp.partition_name) 
      then &#39; %SqlLobPartQuery.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when (tp.subpartition_count &gt; 0)
      then &#39; (%SqlPartSubPartDef.&#39;|| tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name ||&#39;%)&#39;
      else &#39;&#39; end
   as coln
from
   all_part_tables pt
   join all_tab_partitions tp on (tp.table_owner = pt.owner and tp.table_name = pt.table_name)
where 1=1
[  and tp.table_owner=%.q:OWNER% ]
[  and tp.table_name=%.q:MATERIALIZEDVIEW%]
order by 
   1, tp.partition_position
</a:TextTargetItem.Value>
