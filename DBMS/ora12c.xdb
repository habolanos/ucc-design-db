<o:TextTargetItem Id="o391">
<a:ObjectID>D11E1C30-D1D9-4401-84AC-6E1422C9FB83</a:ObjectID>
<a:Name>SqlQuotaQuery</a:Name>
<a:TextTargetItem.Value>{USER ID, QuotaDefinition ...}

[%ISDBAUSER%?
select
   q.username,
   &#39; quota &#39;
   || case when (q.max_bytes = -1) then &#39;unlimited&#39; else 
   case 
      when (mod(q.max_bytes, power(2, 30)) = 0) then
         to_char(q.max_bytes/power(2, 30)) || &#39; G&#39;
      when (mod(q.max_bytes, power(2, 20)) = 0) then
         to_char(q.max_bytes/power(2, 20)) || &#39; M&#39;
      when (mod(q.max_bytes, power(2, 10)) = 0) then
         to_char(q.max_bytes/power(2, 10)) || &#39; K&#39;
      else
         to_char(q.max_bytes)
      end
   end
   || &#39; on &#39; || q.tablespace_name
   as opts
from 
   dba_ts_quotas q
[where q.username = %.q:USER%]]</a:TextTargetItem.Value>
<a:CreationDate>1269424919</a:CreationDate>
<a:Creator>mledier</a:Creator>
