<o:TextTargetItem Id="o502">
<a:ObjectID>782C5382-0A60-4B5C-AD14-F546E71E5D54</a:ObjectID>
<a:Name>SqlPartitionDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   pt.owner||pt.table_name,
   case(pt.PARTITIONING_TYPE)
      when &#39;RANGE&#39; then 
         &#39;partition by range (%SqlPartColumnList.&#39;||pt.owner||pt.table_name||&#39;%) &#39;
         ||case when (pt.interval is not null) then &#39; interval (&#39; || pt.interval || &#39;) store in (&#39; || pt.def_tablespace_name || &#39;)&#39; else &#39;&#39; end
         ||case when (pt.def_subpartition_count &gt; 0) then &#39; %SqlSubPartDef.&#39;||pt.owner||pt.table_name||&#39;%&#39; else &#39;&#39; end
         ||&#39; (%SqlRangePartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;LIST&#39; then 
         &#39;partition by list (%SqlPartColumnList.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
         ||case when (pt.def_subpartition_count &gt; 0) then &#39; %SqlSubPartDef.&#39;||pt.owner||pt.table_name||&#39;%&#39; else &#39;&#39; end
         ||&#39;(%SqlListPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;HASH&#39; then &#39;partition by hash (%SqlPartColumnList.&#39;||pt.owner||pt.table_name||&#39;%) (%SqlHashPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;REFERENCE&#39; then &#39;partition by reference (&#39; ||pt.ref_ptn_constraint_name ||&#39;) (%SqlRefrPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      when &#39;SYSTEM&#39; then &#39;partition by system (%SqlRefrPartDef.&#39;||pt.owner||pt.table_name||&#39;%)&#39;
      else &#39;&#39;
   end as coln
from
   all_part_tables pt
where 1=1
[  and pt.owner=%.q:OWNER% ]
[  and pt.table_name=%.q:MATERIALIZEDVIEW%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
