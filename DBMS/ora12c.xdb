<a:ModificationDate>1384880954</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>The columns listed in this clause are the table columns that correspond to the cluster columns</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)
output &quot;%Get%:Start&quot;
   Dim sget, coln, indx
   indx = 1
   sget = &quot;&quot;
   Do
      coln = obj.GetPhysicalOptionSql(&quot;&lt;physical_properties&gt;/&lt;cluster&gt;/cluster/&lt;column&gt;:&quot; &amp; CStr(indx))
      if(coln = &quot;&quot;) then Exit Do
      sget = sget &amp; &quot;, &quot; &amp; coln
      indx = indx + 1
   Loop

   %Get% = Mid(sget, 3)
output &quot;%Get%:end; value=&quot; &amp; Mid(sget, 3)
End Function

</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
output &quot;%Set%:value=&quot; &amp; value
   obj.DeletePhysicalOption &quot;&lt;physical_properties&gt;/&lt;cluster&gt;/cluster/&lt;column&gt;&quot;
   dim arr, coln, indx
   indx = 1
   for each coln in Split(value, &quot;,&quot;)
      obj.AddPhysicalOption &quot;&lt;physical_properties&gt;/&lt;cluster&gt;/cluster/&lt;column&gt;:&quot; &amp; CStr(indx)
      obj.SetPhysicalOptionSql &quot;&lt;physical_properties&gt;/&lt;cluster&gt;/cluster/&lt;column&gt;:&quot; &amp; CStr(indx), Trim(coln)
      indx = indx + 1
   next
output &quot;%Set%:end&quot;
End Sub

</a:SetMethodScript>
