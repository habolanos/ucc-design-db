<o:TextTargetItem Id="o106">
<a:ObjectID>340970EC-4D60-4A6E-B449-534017F2CCD3</a:ObjectID>
<a:Name>SqlRefrPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   tp.table_owner||tp.table_name,
   decode (tp.partition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; partition &#39;  ||tp.partition_name
   ||decode (tp.pct_free, NULL, &#39;&#39;, 10, &#39;&#39;, 0, &#39;&#39;, &#39; pctfree &#39;  ||tp.pct_free)
   ||decode (tp.pct_used, NULL, &#39;&#39;, 40, &#39;&#39;, 0, &#39;&#39;,  &#39; pctused &#39;  ||tp.pct_used)
   ||decode (tp.ini_trans, NULL, &#39;&#39;, 1, &#39;&#39;, 0, &#39;&#39;,  &#39; initrans &#39;  ||tp.ini_trans)
   ||decode (tp.tablespace_name, &#39;SYSTEM&#39;, &#39;&#39;, NULL, &#39;&#39;, &#39; tablespace &#39;  ||tp.tablespace_name)
   ||decode (tp.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
   ||decode(nvl(tp.initial_extent,nvl(tp.next_extent,nvl(tp.min_extent,nvl(tp.max_extent,nvl(tp.pct_increase,nvl(tp.freelists,nvl(tp.freelist_groups,NULL))))))), NULL, &#39;&#39;, &#39; storage (&#39;
      ||decode (tp.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(tp.initial_extent/1024)||&#39;K&#39;)
      ||decode (tp.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(tp.next_extent/1024)||&#39;K&#39;)
      ||decode (tp.min_extent, NULL, &#39;&#39;, &#39; minextents &#39; ||tp.min_extent)
      ||decode (tp.max_extent, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||tp.max_extent)
      ||decode (tp.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||tp.pct_increase)
      ||decode (tp.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||tp.freelists)
      ||decode (tp.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||tp.freelist_groups)
      ||&#39; )&#39;)
   ||case when (tp.compression=&#39;ENABLED&#39;) then
      &#39; compress &#39; || decode(nvl(tp.compress_for, &#39;&#39;), &#39;FOR ALL OPERATIONS&#39;, &#39;for all operations&#39;, &#39;DIRECT LOAD ONLY&#39;, &#39;for direct_load only&#39;, &#39;&#39;)
     else &#39;&#39; end
   ||case when (o.iot_name is null) then &#39;&#39; else
       &#39; %SqlPartOverflowQry.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
     end
   ||case when exists (select 1 from all_lob_partitions l where l.table_owner = tp.table_owner and l.table_name = tp.table_name and l.partition_name = tp.partition_name and rownum=1) then
       &#39; %SqlLobPartQuery.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
     else &#39;&#39; end
   as coln
from
   all_tab_partitions tp
   left outer join all_tables o on (o.iot_type = &#39;IOT_OVERFLOW&#39; and o.owner = tp.table_owner and o.iot_name = tp.table_name)
where 1=1
[  and tp.table_owner=%.q:OWNER% ]
[  and tp.table_name=%.q:TABLE%]
order by 
   1, tp.partition_position
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400165644</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o107">
<a:ObjectID>D595293A-89C1-4857-ADE0-BEC03C072D46</a:ObjectID>
<a:Name>SqlLobPartQuery</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   l.table_owner||&#39;-&#39;||l.table_name||&#39;-&#39;||l.partition_name,
   decode (l.column_name, NULL, &#39;&#39;, &#39; lob (&#39;||l.column_name||&#39;)&#39;
      ||&#39; store as&#39;
      ||decode(l.securefile, &#39;NO&#39;, &#39; basicfile&#39;, &#39; securefile&#39;)
      ||decode(substr(l.lob_partition_name, 1, 7), &#39;SYS_LOB&#39;, &#39;&#39;, &#39; &#39;||l.lob_partition_name)
      ||&#39; (&#39;
         ||&#39; tablespace &#39; || l.tablespace_name
         ||&#39; chunk &#39;||l.chunk
         ||&#39; pctversion &#39;||l.pctversion
         ||decode (l.deduplication, &#39;LOB&#39;, &#39; deduplicate &#39;, &#39;NO&#39;, &#39; keep_duplicates &#39;, &#39;&#39;)
         ||decode (l.compression, &#39;NO&#39;, &#39; nocompress &#39;, &#39;HIGH&#39;, &#39; compress high &#39;, &#39;MEDIUM&#39;, &#39; compress medium &#39;, &#39;&#39;)
         ||decode (l.encrypt, &#39;NO&#39;, &#39; decrypt &#39;, &#39;YES&#39;, &#39; encrypt &#39;, &#39;&#39;)
         ||decode (l.cache, &#39;NO&#39;, &#39; nocache &#39;, &#39;YES&#39;, &#39; cache &#39;, &#39;CACHEREADS&#39;, &#39; cache reads &#39;)
         ||decode (l.logging, &#39;NO&#39;, &#39; nologging &#39;, &#39;YES&#39;, &#39; logging &#39;)
         ||decode (l.in_row, &#39;NO&#39;, &#39; disable&#39;, &#39;YES&#39;, &#39; enable&#39;)||&#39; storage in row&#39;
      ||&#39; )&#39;) as coln
from 
   all_lob_partitions l
where 1=1
[  and l.table_owner=%.q:OWNER% ]
[  and l.table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1248101256</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o108">
<a:ObjectID>7E28CAE2-27AA-4489-8F57-D7650F8F76DB</a:ObjectID>
<a:Name>SqlPartOverflowQry</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select
   o.owner||&#39;-&#39;||o.iot_name||&#39;-&#39;||p.partition_name,
   &#39; overflow &#39;
   ||decode (p.pct_free, NULL, &#39;&#39;, &#39; pctfree &#39; || p.pct_free)
   ||decode (p.ini_trans, NULL, &#39;&#39;, &#39; initrans &#39; || p.ini_trans)
   ||&#39; storage (&#39;
   ||decode (p.initial_extent, NULL, &#39;&#39;, &#39; initial &#39;|| (p.initial_extent/1024)||&#39;K&#39;)
       ||decode (p.next_extent, NULL, &#39;&#39;, &#39; next &#39;|| (p.next_extent/1024)||&#39;K&#39;)
       ||decode (p.min_extent, NULL, &#39;&#39;, &#39; minextents &#39;|| p.min_extent)
       ||decode (p.max_extent, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39;|| p.max_extent)
       ||decode (p.pct_increase, 0, &#39;&#39;, &#39; pctincrease &#39;|| p.pct_increase)
       ||decode (p.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||p.freelists)
       ||decode (p.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||p.freelist_groups)
       ||&#39; buffer_pool &#39; || p.buffer_pool
       ||&#39; )&#39;
   ||decode (p.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39; || p.tablespace_name)
   ||decode (p.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
   as coln
from
   all_tables o 
   join all_tab_partitions p on (p.table_owner = o.owner and p.table_name = o.table_name)
where 1=1
[  and o.owner=%.q:OWNER% ]
[  and o.iot_name=%.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1337844753</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o109">
<a:ObjectID>498CF171-7A90-4108-B8EF-58B74E99BAB9</a:ObjectID>
<a:Name>SqlSubPartListTemplateQry</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   t.user_name || &#39;-&#39; || t.table_name,
   decode(t.subpartition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; subpartition &#39; || t.subpartition_name
   ||&#39; values (%SqlSubpartTplHighBoundQuery.&#39; || t.user_name || &#39;-&#39; || t.table_name || &#39;-&#39; || t.subpartition_name ||&#39;%)&#39;
   ||case when (t.tablespace_name is null) then &#39;&#39; else &#39; tablespace &#39; || t.tablespace_name end
   as coln
from
   all_subpartition_templates t
where 1=1
[  and t.user_name = %.q:OWNER%]
[  and t.table_name = %.q:TABLE%]
order by 1, t.subpartition_position
</a:TextTargetItem.Value>
