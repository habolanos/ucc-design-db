<o:TextTargetItem Id="o491">
<a:ObjectID>5AF1E5E4-11F9-4E89-AC39-668EAC9B618D</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, MATERIALIZEDVIEW, SCRIPT, COMMENT}

select
   s.owner,
   s.mview_name,
   s.query,
   (select r.comments from sys.all_mview_comments r where r.mview_name = s.mview_name and r.owner = s.owner)
from
   sys.all_mviews s
where 1=1
[  and s.owner = %.q:OWNER%]
[  and s.mview_name = %.q:MATERIALIZEDVIEW%]
order by 1, 2</a:TextTargetItem.Value>
