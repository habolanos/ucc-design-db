<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, JIDXFACT ID, OWNER ID, JIDX ID,  REFRLIST ... }

select distinct
   i.table_owner,
   i.table_name,
   i.owner,
   i.index_name,
   c.outer_table_owner || &#39;.&#39; || c.outer_table_name || &#39;,&#39; || c.inner_table_owner || &#39;.&#39; || c.inner_table_name || &#39;,&#39;
from
   all_indexes i,
   all_join_ind_columns c
where i.join_index = &#39;YES&#39;
   and nvl(i.dropped, &#39;NO&#39;) = &#39;NO&#39;
   and c.index_owner=i.owner
   and c.index_name=i.index_name
[  and i.owner=%.q:SCHEMA%]
order by 1, 2, 3, 4
</a:TextTargetItem.Value>
