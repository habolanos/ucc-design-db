<o:TextTargetItem Id="o88">
<a:ObjectID>11188347-DC88-4742-AEB4-2489B84475FC</a:ObjectID>
<a:Name>SqlListPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   tp.table_owner||tp.table_name,
   decode (tp.partition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; partition &#39;  ||tp.partition_name
   ||&#39; values (%SqlHVQuery.HighVal&#39;||tp.partition_position||&#39;%) &#39;
   ||decode (tp.pct_free, NULL, &#39;&#39;, &#39; pctfree &#39;  ||tp.pct_free)
   ||decode (tp.pct_used, NULL, &#39;&#39;, 40, &#39;&#39;, 0, &#39;&#39;,  &#39; pctused &#39;  ||tp.pct_used)
   ||decode (tp.ini_trans, NULL, &#39;&#39;, 1, &#39;&#39;, 0, &#39;&#39;,  &#39; initrans &#39;  ||tp.ini_trans)
   ||decode (tp.tablespace_name, &#39;SYSTEM&#39;, &#39;&#39;, NULL, &#39;&#39;, &#39; tablespace &#39;  ||tp.tablespace_name)
   ||replace(&#39; storage (&#39;
      ||decode (tp.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(tp.initial_extent/1024)||&#39;K&#39;)
      ||decode (tp.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(tp.next_extent/1024)||&#39;K&#39;)
      ||decode (tp.min_extent, NULL, &#39;&#39;, &#39; minextents &#39; ||tp.min_extent)
      ||decode (tp.max_extent, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||tp.max_extent)
      ||decode (tp.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||tp.pct_increase)
      ||decode (tp.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||tp.freelists)
      ||decode (tp.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||tp.freelist_groups)
      ||&#39; )&#39;, &#39; storage ( )&#39;, &#39;&#39;)
   ||case when (tp.compression=&#39;ENABLED&#39;) then
      &#39; compress &#39; || decode(nvl(tp.compress_for, &#39;&#39;), &#39;FOR ALL OPERATIONS&#39;, &#39;for all operations&#39;, &#39;DIRECT LOAD ONLY&#39;, &#39;for direct_load operations&#39;, &#39;&#39;)
     else &#39;&#39; end
   ||case when (o.iot_name is not null) 
      then &#39; %SqlPartOverflowQry.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when exists (select 1 from all_lob_partitions l where l.table_owner = tp.table_owner and l.table_name = tp.table_name and l.partition_name = tp.partition_name and rownum=1)
      then &#39; %SqlLobPartQuery.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when (tp.subpartition_count &gt; 0)
      then &#39; (%SqlPartSubPartDef.&#39;|| tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name ||&#39;%)&#39;
      else &#39;&#39; end
   as coln
from
   all_tab_partitions tp
   left outer join all_tables o on (o.iot_type = &#39;IOT_OVERFLOW&#39; and o.owner = tp.table_owner and o.iot_name = tp.table_name)
where 1=1
[  and tp.table_owner = %.q:OWNER%]
[  and tp.table_name = %.q:TABLE%]
order by 
   1, tp.partition_position
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400487406</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o89">
<a:ObjectID>B7B9D51D-96CB-4B15-AC2E-86B521DD0280</a:ObjectID>
<a:Name>SqlRangePartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   tp.table_owner||tp.table_name,
   decode (tp.partition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; partition &#39;  ||tp.partition_name
   ||&#39; values less than (%SqlHVQuery.HighVal&#39;||tp.partition_position||&#39;%) &#39;
   ||decode (tp.indexing, NULL, &#39;&#39;, &#39; indexing &#39; || lower(tp.indexing))
   ||decode (tp.pct_free, NULL, &#39;&#39;, 10, &#39;&#39;, 0, &#39;&#39;, &#39; pctfree &#39;  ||tp.pct_free)
   ||decode (tp.pct_used, NULL, &#39;&#39;, 40, &#39;&#39;, 0, &#39;&#39;,  &#39; pctused &#39;  ||tp.pct_used)
   ||decode (tp.ini_trans, NULL, &#39;&#39;, 1, &#39;&#39;, 0, &#39;&#39;,  &#39; initrans &#39;  ||tp.ini_trans)
   ||decode (tp.tablespace_name, &#39;SYSTEM&#39;, &#39;&#39;, NULL, &#39;&#39;, &#39; tablespace &#39;  ||tp.tablespace_name)
   ||decode (tp.logging, &#39;NO&#39;, &#39; nologging&#39;, &#39;YES&#39;, &#39; logging&#39;, &#39;&#39;)
   ||replace(&#39; storage (&#39;
      ||decode (tp.initial_extent, NULL, &#39;&#39;, &#39; initial &#39; ||(tp.initial_extent/1024)||&#39;K&#39;)
      ||decode (tp.next_extent, NULL, &#39;&#39;, &#39; next &#39; ||(tp.next_extent/1024)||&#39;K&#39;)
      ||decode (tp.min_extent, NULL, &#39;&#39;, &#39; minextents &#39; ||tp.min_extent)
      ||decode (tp.max_extent, NULL, &#39;&#39;, 2147483645, &#39; maxextents unlimited&#39;, &#39; maxextents &#39; ||tp.max_extent)
      ||decode (tp.pct_increase, NULL, &#39;&#39;, &#39; pctincrease &#39; ||tp.pct_increase)
      ||decode (tp.freelists, NULL, &#39;&#39;, &#39; freelists &#39;||tp.freelists)
      ||decode (tp.freelist_groups, NULL, &#39;&#39;, &#39; freelist groups &#39;||tp.freelist_groups)
      ||&#39; )&#39;, &#39; storage ( )&#39;, &#39;&#39;)
   ||case (coalesce(tp.compress_for, tp.compression))
      when pt.def_compress_for then &#39;&#39;
      when pt.def_compression then &#39;&#39;
      when &#39;FOR ALL OPERATIONS&#39; then &#39; compress for all operations&#39;
      when &#39;DIRECT LOAD ONLY&#39; then &#39; compress for direct_load only&#39;
      else case when (pt.def_compression = &#39;NONE&#39;) then &#39;&#39; else &#39; nocompress&#39; end
     end
   ||case when (o.iot_name is not null) 
      then &#39; %SqlPartOverflowQry.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when exists (select 1 from all_lob_partitions l where l.table_owner = tp.table_owner and l.table_name = tp.table_name and l.partition_name = tp.partition_name and rownum=1)
      then &#39; %SqlLobPartQuery.&#39; || tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name || &#39;%&#39; 
      else &#39;&#39; end
   ||case when (tp.subpartition_count &gt; 0)
      then &#39; (%SqlPartSubPartDef.&#39;|| tp.table_owner||&#39;-&#39;||tp.table_name||&#39;-&#39;||tp.partition_name ||&#39;%)&#39;
      else &#39;&#39; end
   as coln
from
   all_part_tables pt
   join all_tab_partitions tp on (tp.table_owner = pt.owner and tp.table_name = pt.table_name)
   left outer join all_tables o on (o.iot_type = &#39;IOT_OVERFLOW&#39; and o.owner = tp.table_owner and o.iot_name = tp.table_name)
where 1=1
[  and pt.owner = %.q:OWNER%]
[  and pt.table_name = %.q:TABLE%]
order by 
   1, tp.partition_position
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400165252</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o90">
<a:ObjectID>2A7C5E25-33FB-44F7-8B1D-476004F148BF</a:ObjectID>
<a:Name>SqlHashPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   p.table_owner||p.table_name,
   decode (p.partition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; partition &#39;  ||p.partition_name
   ||decode (p.tablespace_name, NULL, &#39;&#39;, &#39; tablespace &#39;  ||p.tablespace_name)
   ||case when (o.iot_name is null) then &#39;&#39; else
      &#39; %SqlPartOverflowQry.&#39; || p.table_owner||&#39;-&#39;||p.table_name||&#39;-&#39;||p.partition_name || &#39;%&#39; 
      end
   ||case when (p.compression = &#39;ENABLED&#39;) then &#39; compress&#39;
      ||case (p.compress_for) 
         when &#39;FOR ALL OPERATIONS&#39; then &#39; for all operations&#39;
         when &#39;DIRECT LOAD ONLY&#39; then &#39; for direct_load operations&#39;
        else &#39;&#39; end
     else &#39; nocompress&#39; end
   ||case when exists(select 1 from all_lob_partitions l where l.table_owner = p.table_owner and l.table_name = p.table_name and l.partition_name = p.partition_name) then
      &#39; %SqlLobPartQuery.&#39; || p.table_owner||&#39;-&#39;||p.table_name||&#39;-&#39;||p.partition_name || &#39;%&#39; 
     else &#39;&#39; end
   as coln
from
   all_tab_partitions p
   left outer join all_tables o on (o.iot_type = &#39;IOT_OVERFLOW&#39; and o.owner = p.table_owner and o.iot_name = p.table_name) 
where p.high_value_length = 0
[  and p.table_owner=%.q:OWNER% ]
[  and p.table_name=%.q:TABLE%]
order by 
   1, p.partition_position
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400164991</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o91">
<a:ObjectID>1549D66C-B6D4-46C5-A9AD-5B7E733182EE</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COMMENT, TABLTYPE, CLASSOWNER, CLASS, XMLSCHEMA EX, XMLELEMENT EX}

select
   t.owner,
   t.table_name,
   r.comments,
   case 
      when (t.temporary = &#39;Y&#39;) then &#39;TEMP&#39;
      when (t.table_type = &#39;XMLTYPE&#39;) then &#39;XML&#39;
      when (t.table_type != &#39;&#39;) then &#39;OBJECT&#39;
      else &#39;&#39;
   end,
   t.table_type_owner,
   t.table_type,
   decode (t.table_type, &#39;XMLTYPE&#39;, &#39;%SqlXMLTable.&#39;||t.owner||t.table_name||&#39;1%&#39;, &#39;&#39;),
   decode (t.table_type, &#39;XMLTYPE&#39;, &#39;%SqlXMLTable.&#39;||t.owner||t.table_name||&#39;2%&#39;, &#39;&#39;)
from
   sys.all_tab_comments r,
   sys.all_all_tables t
where
   not exists (select 1 from sys.all_mviews s where s.owner = t.owner and t.table_name in (s.mview_name, s.update_log))
   and t.table_name = r.table_name
   and t.owner = r.owner
   and nvl(t.dropped, &#39;NO&#39;) = &#39;NO&#39;
[  and t.table_name=%.q:TABLE%]
[  and t.owner=%.q:OWNER%]
order by
   t.owner, t.table_name
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1269936675</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to reverse object attributes
SqlXMLTable sub item is necessary for the query performance (using a left outer join is too slow)</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o92">
<a:ObjectID>69613950-7201-48FC-A194-8838B8D04449</a:ObjectID>
<a:Name>SqlSubPartDef</a:Name>
<a:TextTargetItem.Value>{VAR, VAL EX}

select
   p.owner||p.table_name,
   &#39;subpartition by &#39; || lower(p.subpartitioning_type)
   ||&#39;(&#39;
   ||case when (p.subpartitioning_key_count = 1) 
      then (select c.column_name from all_subpart_key_columns c where c.owner = p.owner and c.name = p.table_name and c.object_type = &#39;TABLE&#39;)
      else &#39;%SqlSubPartColumnList.&#39;|| p.owner || &#39;-&#39; || p.table_name || &#39;%&#39;
     end
   ||&#39;)&#39;
   ||case when (t.table_name is null) then &#39;&#39; else
       case (p.subpartitioning_type) 
         when &#39;LIST&#39; then &#39; subpartition template (%SqlSubPartListTemplateQry.&#39; || p.owner||&#39;-&#39;||p.table_name|| &#39;%)&#39;
         when &#39;RANGE&#39; then &#39; subpartition template (%SqlSubPartRangeTemplateQry.&#39; || p.owner||&#39;-&#39;||p.table_name|| &#39;%)&#39;
       else &#39;&#39; end
     end
   as coln
from
   all_part_tables p
   left outer join all_subpartition_templates t on (t.user_name = p.owner and t.table_name = p.table_name and rownum=1)
where p.subpartitioning_type != &#39;NONE&#39;
[  and p.owner=%.q:OWNER% ]
[  and p.table_name=%.q:TABLE%]</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400165714</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o93">
<a:ObjectID>28D9E8C6-5068-410F-ABC8-CC308824B9B0</a:ObjectID>
<a:Name>SqlPartSubPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   s.table_owner||&#39;-&#39;||s.table_name||&#39;-&#39;||s.partition_name,
   decode(s.subpartition_position, 1, &#39;&#39;, &#39;,&#39;)
   ||&#39; subpartition &#39; || s.subpartition_name
   ||case(p.subpartitioning_type)
       when &#39;LIST&#39; then &#39; values (%SqlSubpartHighBoundQuery.&#39; || s.table_owner || &#39;-&#39; || s.table_name || &#39;-&#39; || s.subpartition_name || &#39;%)&#39;
       when &#39;RANGE&#39; then &#39; values less than (%SqlSubpartHighBoundQuery.&#39; || s.table_owner || &#39;-&#39; || s.table_name || &#39;-&#39; || s.subpartition_name || &#39;%)&#39;
     else &#39;&#39; end
   ||&#39; tablespace &#39;  ||s.tablespace_name
   ||case when (s.compression = &#39;ENABLED&#39;) then &#39; compress&#39;
      ||case (s.compress_for) 
         when &#39;FOR ALL OPERATIONS&#39; then &#39; for all operations&#39;
         when &#39;DIRECT LOAD ONLY&#39; then &#39; for direct_load operations&#39;
        else &#39;&#39; end
     else &#39; nocompress&#39; end
   || case when exists(
        select 1 from all_lob_subpartitions l 
        where rownum = 1
          and l.table_owner = s.table_owner 
          and l.table_name = s.table_name
          and l.subpartition_name = s.subpartition_name) then &#39; %SqlSubPartLobStorage.&#39;||s.subpartition_name||&#39;% &#39;
      else &#39;&#39; end
   as coln
from 
   all_tab_subpartitions s
   join all_part_tables p on (p.owner = s.table_owner and p.table_name = s.table_name)
where 1=1
[  and s.table_owner=%.q:OWNER% ]
[  and s.table_name=%.q:TABLE%]
order by
   1, s.subpartition_position</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400487463</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o94">
<a:ObjectID>02306BAB-1DB4-4B63-9ADA-F1FD5CACB6AF</a:ObjectID>
<a:Name>SqlSubPartColumnList</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   c.owner||c.name,
   case when (c.column_position =1) then c.column_name else &#39;, &#39; || c.column_name end
from
   all_subpart_key_columns c
where
   c.object_type like &#39;TABLE%&#39;
[  and c.owner=%.q:OWNER%]
[  and c.name=%.q:TABLE%]
order by 
   1, c.column_position</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
