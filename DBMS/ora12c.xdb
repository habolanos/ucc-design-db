<o:TextTargetItem Id="o79">
<a:ObjectID>CBFEA2B2-8D13-461B-AF98-5888DCAA3598</a:ObjectID>
<a:Name>SqlLobStorage</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   l.owner||l.table_name,
   &#39; lob (&#39;||l.column_name||&#39;)&#39;
   ||&#39; store as&#39;
   ||decode(l.securefile, &#39;NO&#39;, &#39; basicfile&#39;, &#39; securefile&#39;)
   ||decode(substr(l.segment_name, 1, 7), &#39;SYS_LOB&#39;, &#39;&#39;, &#39; &#39;||l.segment_name)
   ||&#39; (&#39;
   ||&#39; tablespace &#39; || l.tablespace_name
   ||&#39; chunk &#39;||l.chunk
   ||decode(l.pctversion, null, &#39;&#39;, &#39; pctversion &#39;||l.pctversion)
   ||decode(l.retention, null, &#39;&#39;, &#39; retention&#39;)
   ||decode (l.cache, &#39;NO&#39;, &#39; nocache &#39;, &#39;YES&#39;, &#39; cache &#39;)
   ||decode (l.logging, &#39;NO&#39;, &#39; nologging &#39;, &#39;YES&#39;, &#39; logging &#39;)
   ||decode (l.in_row, &#39;NO&#39;, &#39; disable&#39;, &#39;YES&#39;, &#39; enable&#39;)||&#39; storage in row&#39;
   ||&#39; )&#39; as coln 
from 
   all_lobs l
where 
   l.column_name is not null
   and l.partitioned = &#39;NO&#39;   
[  and l.owner=%.q:OWNER% ]
[  and l.table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1400165142</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o80">
<a:ObjectID>EA193481-0942-449C-A47E-1B74FC92806C</a:ObjectID>
<a:Name>SqlNestedTable</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   n.owner||n.parent_table_name,   
   &#39; nested table &#39;||n.parent_table_column||&#39; store as &#39;||n.table_name ||&#39;,&#39;
from
   all_nested_tables n
where 1=1
[  and n.owner=%.q:OWNER% ]
[  and n.parent_table_name=%.q:TABLE%]
</a:TextTargetItem.Value>
