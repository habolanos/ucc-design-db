<o:TextTargetItem Id="o506">
<a:ObjectID>92C5DBA0-FF79-47B7-99D4-D9037B9B9996</a:ObjectID>
<a:Name>SqlSubPartListTemplateQry</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   t.user_name || &#39;-&#39; || t.table_name,
   decode(t.subpartition_position, 1, &#39;&#39;, &#39;, &#39;)
   ||&#39; subpartition &#39; || t.subpartition_name
   ||&#39; values (%SqlSubpartTplHighBoundQuery.&#39; || t.user_name || &#39;-&#39; || t.table_name || &#39;-&#39; || t.subpartition_name ||&#39;%)&#39;
   ||case when (t.tablespace_name is null) then &#39;&#39; else &#39; tablespace &#39; || t.tablespace_name end
   as coln
from
   all_subpartition_templates t
where 1=1
[  and t.user_name = %.q:OWNER%]
[  and t.table_name = %.q:MATERIALIZEDVIEW%]
order by 1, t.subpartition_position
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
