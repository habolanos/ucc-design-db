<a:Modifier>I063968</a:Modifier>
<a:Comment>Sql text for editionable and public options</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)
   
   dim pub, edt, sget
   pub = CBool(obj.Public)
   edt = CBool(obj.GetExtendedAttribute(&quot;Editionable&quot;))
   if(pub and edt) then
      sget = &quot;editionable public&quot;
   elseif(pub) then
      sget = &quot;public&quot;
   elseif not(edt) then
      sget = &quot;noneditionable&quot;
   else
      sget = &quot;&quot;
   end if
   %Get% = sget
   
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)

   dim pub, edt
   pub = (InStr(LCase(value), &quot;public&quot;) &gt; 0)
   if(pub) then
      edt = (Left(value, Len(&quot;editionable&quot;)) = &quot;editionable&quot;)
   else
      edt = (Left(value, Len(&quot;noneditionable&quot;)) &lt;&gt; &quot;noneditionable&quot;)
   end if
   obj.Public = pub
   obj.SetExtendedAttribute &quot;Editionable&quot;, edt
   
End Sub</a:SetMethodScript>
<a:Label>SynScope</a:Label>
