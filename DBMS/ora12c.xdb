<a:Name>CRUD DatabasePackage</a:Name>
<a:Code>CRUD DatabasePackage</a:Code>
<a:Text>create package %Table.Name%_PKG as
    TYPE ref_cursor IS REF CURSOR;

.if (%canDefineDeleteProcedure%)
  procedure DEL
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;);\n&quot;, %Primary%)
p_%Code% IN %parameterDataType%
    .next(&quot;,&quot;)
.endif(&quot;&quot;)

.if (%canDefineInsertProcedure%)
  procedure INS
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;);\n&quot;)
        .if (%SerialDataType%)
p_%Code% OUT %parameterDataType%
        .else
p_%Code% IN %parameterDataType%
        .endif
    .next(&quot;,&quot;)
.endif(&quot;&quot;)

.if (%canDefineSelectProcedure%)
  function SEL
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;)\n&quot;, %Primary%)
p_%Code% IN %parameterDataType%
    .next(&quot;,&quot;)
RETURN ref_cursor;
.endif(&quot;\n&quot;)

.if (%canDefineUpdateProcedure%)
  procedure UPD
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;);\n&quot;)
p_%Code% IN %parameterDataType%
    .next(&quot;,&quot;)
.endif(&quot;&quot;)
end %Table.Name%_PKG
/


create or replace package body %Table.Name%_PKG as


.if (%canDefineDeleteProcedure%)
  procedure DEL
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;) as\n&quot;, %Primary%)
p_%Code% IN %parameterDataType%
    .next(&quot;,&quot;)
begin
delete from 
    .if(%Owner%)
%Owner.Code%.%Table.GeneratedName%
    .else
%Table.GeneratedName%
    .endif(&quot;\n&quot;)
    .foreach_item(Table.Columns,&quot;where &quot;,&quot;;\n&quot;, %Primary%)
%GeneratedName% = p_%Code%
    .next(&quot; and &quot;)
end;
.endif(&quot;\n\n&quot;)

.if (%canDefineInsertProcedure%)
  procedure INS
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;) as\n&quot;)
        .if (%SerialDataType%)
p_%Code% OUT %parameterDataType%
        .else
p_%Code% IN %parameterDataType%
        .endif
    .next(&quot;,&quot;)
begin

    .if (%Table.hasSequenceWithoutTrigger%)
        .foreach_item(Table.Columns,&quot;&quot;,&quot;;\n&quot;, %SerialDataType%)
select %Sequence.Code%.nextval into p_%Code% from dual
        .next(&quot;,&quot;)
    .endif(&quot;\n&quot;)
insert into 
    .if(%Owner%)
%Owner.Code%.%Table.GeneratedName%
    .else
%Table.GeneratedName%
    .endif()
    .if (%Table.hasSequenceWithoutTrigger%)
        .foreach_item(Table.Columns,&quot; (&quot;,&quot;)\n&quot;)
%GeneratedName%
        .next(&quot;,&quot;)
    .else
        .foreach_item(Table.Columns,&quot; (&quot;,&quot;)\n&quot;, %SerialDataType% == false)
%GeneratedName%
        .next(&quot;,&quot;)
    .endif
values
    .if (%Table.hasSequenceWithoutTrigger%)
        .foreach_item(Table.Columns,&quot; (&quot;,&quot;);\n&quot;)
p_%Code%
        .next(&quot;,&quot;)
    .else
        .foreach_item(Table.Columns,&quot; (&quot;,&quot;);\n&quot;, %SerialDataType% == false)
p_%Code%
        .next(&quot;,&quot;)
    .endif(&quot;\n&quot;)

    .if (%Table.hasSequenceTrigger%)
        .foreach_item(Table.Columns,&quot;&quot;,&quot;;\n&quot;, %SerialDataType%)
select %Sequence.Code%.currval into p_%Code% from dual
        .next(&quot;,&quot;)
    .endif(&quot;\n&quot;)
end;
.endif(&quot;\n\n&quot;)

.if (%canDefineSelectProcedure%)
  function SEL
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;) \n&quot;, %Primary%)
p_%Code% IN %parameterDataType%
    .next(&quot;,&quot;)
RETURN ref_cursor as
my_cursor ref_cursor;
begin
open my_cursor for
select * from 
    .if(%Owner%)
%Owner.Code%.%Table.GeneratedName%
    .else
%Table.GeneratedName%
    .endif(&quot;\n&quot;)
    .foreach_item(Table.Columns,&quot;where &quot;,&quot;;\n&quot;, %Primary%)
%GeneratedName% = p_%Code%
    .next(&quot; and &quot;)
return my_cursor;
end;
.endif(&quot;\n\n&quot;)

.if (%canDefineUpdateProcedure%)
