   c.column_id,
   c.column_name,
   t.comments
from
   sys.all_objects o
   join sys.all_tab_columns c on (c.owner = o.owner and c.table_name = o.object_name)
   join sys.all_col_comments t on (t.column_name = c.column_name and t.owner = o.owner and t.table_name = o.object_name)
where o.object_type in (&#39;VIEW&#39;, &#39;MATERIALIZED VIEW&#39;)
[  and o.owner = %.q:OWNER%]
[  and o.object_name = %.q:VIEW%]
[  and c.column_name = %.q:COLUMN%]
order by 1, 2, 3
